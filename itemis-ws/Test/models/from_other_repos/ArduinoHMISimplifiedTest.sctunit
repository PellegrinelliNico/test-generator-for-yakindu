testclass ArduinoHMISimplifiedTest for statechart ArduinoHMI {
	@Test
	operation test01 () {
	    enter
	    raise right
	    proceed 1 cycle
	    raise left
	    proceed 1 cycle
	    assert is_active
	}

	@Test
	operation test02 () {
	    enter
	    proceed 1 cycle
	    assert is_active
	}

	@Test
	operation test03 () {
	    exit
	    assert !is_final
	}

	@Test
	operation test04 () {
	    enter
	    raise right
	    proceed 1 cycle
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test05 () {
	    enter
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test06 () {
	    enter
	    assert is_active
	}

	@Test
	operation test07 () {
	    enter
	    raise select
	    proceed 1 cycle
	    assert is_active
	    assert active (ArduinoHMI.hmi.Handle_States.r1.Handle_States_Active.handle_states.Top)
	}

	@Test
	operation test08 () {
	    enter
	    assert is_active
	    assert !active (ArduinoHMI.hmi.Handle_States.r1.Handle_States_Active)
	}

	@Test
	operation test09 () {
	    enter
	    assert is_active
	    assert active (ArduinoHMI.hmi.Handle_States.r1.Selecting)
	}

	@Test
	operation test10 () {
	}

	@Test
	operation test11 () {
	    assert !active (ArduinoHMI.hmi.Counter.r1.Counter_Active)
	}

	@Test
	operation test12 () {
	}

	@Test
	operation test13 () {
	    assert !active (ArduinoHMI.hmi.Stopwatch.r1.Stopwatch_Active)
	}

	@Test
	operation test14 () {
	    assert !active (ArduinoHMI.hmi.Stopwatch)
	}

	@Test
	operation test15 () {
	    assert !active (ArduinoHMI.hmi.Handle_States.r1.Handle_States_Active)
	}

	@Test
	operation test16 () {
	    assert !active (ArduinoHMI.hmi.Handle_States)
	}

	@Test
	operation test17 () {
	    assert !active (ArduinoHMI.hmi.Counter.r1.Counter_Active.counter.Count)
	}

	@Test
	operation test18 () {
	    assert !active (ArduinoHMI.hmi.Counter.r1.Selecting)
	}

	@Test
	operation test19 () {
	    assert !active (ArduinoHMI.hmi.Stopwatch.r1.Stopwatch_Active.stopwatch.Stop_Timer)
	}

	@Test
	operation test20 () {
	    assert !active (ArduinoHMI.hmi.Stopwatch.r1.Stopwatch_Active.stopwatch.Start_Timer)
	}

	@Test
	operation test21 () {
	    assert !active (ArduinoHMI.hmi.Stopwatch.r1.Stopwatch_Active.stopwatch.Reset_Timer)
	}

	@Test
	operation test22 () {
	    assert !active (ArduinoHMI.hmi.Stopwatch.r1.Selecting)
	}

	@Test
	operation test23 () {
	    assert !active (ArduinoHMI.hmi.Handle_States.r1.Handle_States_Active.handle_states.Right)
	}

	@Test
	operation test24 () {
	    assert !active (ArduinoHMI.hmi.Handle_States.r1.Handle_States_Active.handle_states.Bottom)
	}

	@Test
	operation test25 () {
	    assert !active (ArduinoHMI.hmi.Handle_States.r1.Handle_States_Active.handle_states.Left)
	}

	@Test
	operation test26 () {
	    assert !active (ArduinoHMI.hmi.Handle_States.r1.Handle_States_Active.handle_states.Middle)
	}

	@Test
	operation test27 () {
	    assert !active (ArduinoHMI.hmi.Handle_States.r1.Handle_States_Active.handle_states.Top)
	}

	@Test
	operation test28 () {
	    assert !active (ArduinoHMI.hmi.Counter.r1.Counter_Active.counter.Reset_Counter)
	}

	@Test
	operation test29 () {
	    assert !active (ArduinoHMI.hmi.Handle_States.r1.Selecting)
	}

	@Test
	operation test30 () {
	    proceed 1 cycle
	    assert !is_active
	}

	@Test
	operation test31 () {
	    raise select
	    assert !is_active
	    enter
	    proceed 1 cycle
	    proceed 1 cycle
	    assert is_active
	}

	@Test
	operation test32 () {
	    assert !is_active
	    enter
	    assert is_active
	}

	@Test
	operation test33 () {
	    assert !is_active
	}

	@Test
	operation test38 () {
	    assert !is_active
	    enter
	    raise right
	    proceed 1 cycle
	    proceed 1 cycle
	    assert is_active
	}

	@Test
	operation test39 () {
	    assert !is_active
	    assert !is_final
	}

	@Test
	operation test40 () {
	    raise up
	    assert !is_active
	}

	@Test
	operation test41 () {
	    proceed 100ms
	    assert !is_active
	}

	@Test
	operation test42 () {
	    raise down
	    assert !is_active
	}

	@Test
	operation test43 () {
	    enter
	    raise right
	    proceed 1 cycle
	    raise right
	    proceed 1 cycle
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test44 () {
	    raise select
	    enter
	    proceed 1 cycle
	    assert is_active
	    exit
	    assert !is_active
	}

}