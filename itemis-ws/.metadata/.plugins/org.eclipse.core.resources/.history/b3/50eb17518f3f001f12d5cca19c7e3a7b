testclass TempSimplifiedTest for statechart Stc {
	@Test
	operation test00 () {
	    raise myEvent: 1
	    enter
	    proceed 1 cycle
	    proceed 1 cycle
	    assert is_active
	}

	@Test
	operation test01 () {
	    enter
	    proceed 1 cycle
	    assert is_active
	}

	@Test
	operation test02 () {
	    raise myEvent: 1
	    enter
	    proceed 1 cycle
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test03 () {
	    enter
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test04 () {
	    raise myEvent: 1
	    enter
	    proceed 1 cycle
	    assert is_active
	    assert active (Temp.main_region.StateB)
	}

	@Test
	operation test05 () {
	    enter
	    assert is_active
	    assert active (Temp.main_region.StateA)
	}

	@Test
	operation test06 () {
	}

	@Test
	operation test07 () {
	    assert !active (Temp.main_region.StateB)
	}

	@Test
	operation test08 () {
	    assert !active (Temp.main_region.StateA)
	}

	@Test
	operation test09 () {
	    proceed 1 cycle
	    assert !is_final
	}

	@Test
	operation test10 () {
	    enter
	    assert is_active
	}

	@Test
	operation test11 () {
	    assert !is_active
	}

	@Test
	operation test12 () {
	    exit
	    assert !is_final
	}

	@Test
	operation test13 () {
	    assert !is_final
	    assert !is_active
	}

	@Test
	operation test14 () {
	    assert !is_active
	    enter
	    raise myEvent: 79
	    proceed 1 cycle
	    raise intf.myIntfEvent: " T/Q,QTD"
	    proceed 1 cycle
	    assert is_active
	}

	@Test
	operation test15 () {
	    raise intf.myIntfEvent: "az.9dCA"
	}

}