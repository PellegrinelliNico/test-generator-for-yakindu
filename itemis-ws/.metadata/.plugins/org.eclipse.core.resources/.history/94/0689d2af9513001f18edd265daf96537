testclass Ultrasonic_SensorTest for statechart Ultrasonic_Sensor {
	@Test
	operation test00 () {
	}

	@Test
	operation test05 () {
	    enter
	    enter
	    assert is_active
	}

	@Test
	operation test06 () {
	    triggerWithoutEvent
	    assert !is_active
	}

	@Test
	operation test07 () {
	}

	@Test
	operation test08 () {
	}

	@Test
	operation test09 () {
	    enter
	    proceed 1s
	    assert is_active
	}

	@Test
	operation test10 () {
	    enter
	    proceed 1s
	    assert is_active
	}

	@Test
	operation test11 () {
	    enter
	    proceed 1s
	    assert !is_final
	}

	@Test
	operation test12 () {
	    exit
	    assert !is_active
	}

	@Test
	operation test13 () {
	    enter
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test14 () {
	    assert !is_active
	    enter
	    raise off
	    assert is_active
	    assert active (Ultrasonic_Sensor._Ultrasonic_Sensor_._off_)
	}

	@Test
	operation test15 () {
	    assert !is_active
	    enter
	    assert is_active
	    assert active (Ultrasonic_Sensor._Ultrasonic_Sensor_._Sensor_Monitoring_.UltrasonicReceiver.Receiving)
	}

	@Test
	operation test16 () {
	    assert !is_active
	    enter
	    assert is_active
	    assert active (Ultrasonic_Sensor._Ultrasonic_Sensor_._Sensor_Monitoring_.UltrasonicTransducer.Transmitting)
	}

	@Test
	operation test17 () {
	    assert !is_active
	    enter
	    assert active (Ultrasonic_Sensor._Ultrasonic_Sensor_._Sensor_Monitoring_)
	}

	@Test
	operation test18 () {
	    assert !is_active
	}

	@Test
	operation test19 () {
	    assert !is_active
	    assert !active (Ultrasonic_Sensor._Ultrasonic_Sensor_._Sensor_Monitoring_)
	}

	@Test
	operation test20 () {
	    assert !is_active
	    assert !active (Ultrasonic_Sensor._Ultrasonic_Sensor_._Sensor_Monitoring_.UltrasonicReceiver.Receiving)
	}

	@Test
	operation test21 () {
	    assert !active (Ultrasonic_Sensor._Ultrasonic_Sensor_._off_)
	    assert !is_active
	}

	@Test
	operation test22 () {
	    assert !is_active
	    assert !active (Ultrasonic_Sensor._Ultrasonic_Sensor_._Sensor_Monitoring_.UltrasonicTransducer.Transmitting)
	}

	@Test
	operation test23 () {
	    assert !is_active
	    enter
	    raise off
	    raise on
	    assert is_active
	}

	@Test
	operation test24 () {
	    enter
	    raise off
	}

	@Test
	operation test25 () {
	    assert !is_active
	}

	@Test
	operation test27 () {
	    assert !is_active
	}

	@Test
	operation test28 () {
	    assert !is_active
	}

	@Test
	operation test31 () {
	    assert !is_final
	    assert !is_active
	}

	@Test
	operation test32 () {
	    assert !is_active
	}

	@Test
	operation test34 () {
	    enter
	    raise off
	    exit
	    assert !is_active
	}

	@Test
	operation test35 () {
	    assert !is_active
	    enter
	    assert is_active
	}

}