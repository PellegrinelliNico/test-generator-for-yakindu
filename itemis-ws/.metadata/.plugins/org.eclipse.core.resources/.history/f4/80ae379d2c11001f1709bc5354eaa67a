testclass Statechart12Test for statechart Statechart12 {
	@Test
	operation test00 () {
	    triggerWithoutEvent
	    assert !is_final
	}

	@Test
	operation test01 () {
	    raise carDetected
	    assert !is_final
	}

	@Test
	operation test07 () {
	    enter
	    raise buttonPressed
	    raise buttonPressed
	    assert is_active
	}

	@Test
	operation test08 () {
	    enter
	    proceed 500ms
	    assert is_active
	}

	@Test
	operation test09 () {
	    enter
	    raise buttonPressed
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test10 () {
	    exit
	    assert !is_active
	}

	@Test
	operation test11 () {
	    assert !is_active
	    enter
	    assert is_active
	    assert active (Statechart12.main_region.TrafficLight.toggleButtonHandling.ToggleModeUnpressed)
	}

	@Test
	operation test12 () {
	    assert !is_active
	    enter
	    assert is_active
	}

	@Test
	operation test13 () {
	    assert !is_active
	    enter
	    assert is_active
	    assert active (Statechart12.main_region.TrafficLight.trafficLightsHandling.NormalBehavior.r1.red)
	}

	@Test
	operation test14 () {
	    assert !is_active
	    enter
	    assert is_active
	    assert active (Statechart12.main_region.TrafficLight.trafficLightsHandling.NormalBehavior)
	}

	@Test
	operation test15 () {
	    assert !is_active
	    enter
	    assert is_active
	    assert active (Statechart12.main_region.TrafficLight)
	}

	@Test
	operation test16 () {
	    assert !is_active
	}

	@Test
	operation test17 () {
	    assert !is_active
	}

	@Test
	operation test18 () {
	    assert !active (Statechart12.main_region.TrafficLight.trafficLightsHandling.NormalBehavior)
	    assert !is_active
	}

	@Test
	operation test19 () {
	    assert !active (Statechart12.main_region.TrafficLight.trafficLightsHandling.PoliceInterruptBehavior)
	    assert !is_active
	}

	@Test
	operation test20 () {
	    assert !active (Statechart12.main_region.TrafficLight)
	    assert !is_active
	}

	@Test
	operation test21 () {
	    assert !is_active
	}

	@Test
	operation test22 () {
	    assert !is_active
	}

	@Test
	operation test23 () {
	    assert !is_active
	}

	@Test
	operation test24 () {
	    assert !is_active
	}

	@Test
	operation test25 () {
	    assert !is_active
	    assert !active (Statechart12.main_region.TrafficLight.trafficLightsHandling.NormalBehavior.r1.red)
	}

	@Test
	operation test26 () {
	    assert !active (Statechart12.main_region.TrafficLight.trafficLightsHandling.PoliceInterruptBehavior.r1.YellowOff)
	    assert !is_active
	}

	@Test
	operation test27 () {
	    assert !is_active
	}

	@Test
	operation test28 () {
	    assert !is_active
	    assert !active (Statechart12.main_region.TrafficLight.trafficLightsHandling.PoliceInterruptBehavior.r1.YellowOn)
	}

	@Test
	operation test29 () {
	    assert !is_active
	}

	@Test
	operation test30 () {
	    assert !is_active
	    enter
	    assert is_active
	}

	@Test
	operation test32 () {
	    enter
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test33 () {
	    assert !is_final
	    assert !is_active
	}

	@Test
	operation test36 () {
	    enter
	    raise buttonPressed
	    raise buttonReleased
	    assert !is_final
	}

	@Test
	operation test37 () {
	    assert !is_active
	    enter
	    raise buttonPressed
	    assert active (Statechart12.main_region.TrafficLight.toggleButtonHandling.PressedShort)
	}

}