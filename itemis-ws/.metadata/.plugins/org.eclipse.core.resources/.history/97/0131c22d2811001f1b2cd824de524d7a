testclass motionDetectionTest for statechart motionDetection {
	@Test
	operation test00 () {
	    proceed 30s
	    assert !is_final
	}

	@Test
	operation test01 () {
	}

	@Test
	operation test02 () {
	}

	@Test
	operation test03 () {
	}

	@Test
	operation test04 () {
	}

	@Test
	operation test06 () {
	}

	@Test
	operation test07 () {
	}

	@Test
	operation test08 () {
	}

	@Test
	operation test09 () {
	    assert !active (motionDetection.main_region.AutomaticMode.r1.MotionDetectionMode)
	}

	@Test
	operation test10 () {
	    assert !active (motionDetection.main_region.AutomaticMode.r1.TwilightDetectionMode)
	}

	@Test
	operation test11 () {
	    assert !active (motionDetection.main_region.AutomaticMode)
	}

	@Test
	operation test12 () {
	    assert !active (motionDetection.main_region.AutomaticMode.r1.MotionDetectionMode.r1.MotionDetected)
	}

	@Test
	operation test13 () {
	    assert !active (motionDetection.main_region.AutomaticMode.r1.MotionDetectionMode.r1.Idle)
	}

	@Test
	operation test14 () {
	    assert !active (motionDetection.main_region.AutomaticMode.r1.TwilightDetectionMode.r1.Idle)
	}

	@Test
	operation test15 () {
	    assert !active (motionDetection.main_region.AutomaticMode.r1.TwilightDetectionMode.r1.TwilightDetected)
	}

	@Test
	operation test16 () {
	    assert !active (motionDetection.main_region.ManualMode)
	}

	@Test
	operation test17 () {
	    assert !is_active
	}

	@Test
	operation test18 () {
	    exit
	    assert !is_active
	}

	@Test
	operation test19 () {
	    exit
	    assert !is_final
	}

	@Test
	operation test22 () {
	    raise lum_sensor.bright
	}

	@Test
	operation test23 () {
	    assert !is_active
	}

	@Test
	operation test24 () {
	    raise lum_sensor.dark
	    assert !is_active
	}

	@Test
	operation test25 () {
	    assert !is_active
	}

	@Test
	operation test26 () {
	    assert !is_active
	}

	@Test
	operation test27 () {
	    assert !is_active
	}

	@Test
	operation test28 () {
	    assert !is_active
	}

	@Test
	operation test29 () {
	    assert !is_active
	    assert !is_final
	}

	@Test
	operation test30 () {
	    assert !is_active
	}

	@Test
	operation test32 () {
	    raise user.off_button
	    assert !is_active
	}

	@Test
	operation test33 () {
	    raise user.mode_button
	    assert !is_active
	}

	@Test
	operation test34 () {
	    raise user.on_button
	    assert !is_active
	}

	@Test
	operation test35 () {
	    raise sensor.motion
	    assert !is_active
	}

}