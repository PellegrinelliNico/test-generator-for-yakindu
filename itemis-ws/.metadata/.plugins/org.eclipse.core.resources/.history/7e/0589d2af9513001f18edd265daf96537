testclass Sensor_ComponentTest for statechart Sensor_Component {
	@Test
	operation test00 () {
	    triggerWithoutEvent
	    assert !is_final
	}

	@Test
	operation test01 () {
	    assert !is_final
	}

	@Test
	operation test06 () {
	    enter
	    raise off
	    raise off
	    assert is_active
	}

	@Test
	operation test07 () {
	    enter
	    proceed 1s
	    assert is_active
	}

	@Test
	operation test08 () {
	    enter
	    raise off
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test09 () {
	    enter
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test10 () {
	    enter
	    raise off
	    assert is_active
	    assert active (Sensor_Component._Sensor_Component_._off_)
	}

	@Test
	operation test11 () {
	    enter
	    assert is_active
	    assert active (Sensor_Component._Sensor_Component_._Sensor_Monitoring_._Sensing_.Under_Threshold)
	}

	@Test
	operation test12 () {
	    enter
	    assert is_active
	    assert active (Sensor_Component._Sensor_Component_._Sensor_Monitoring_)
	}

	@Test
	operation test13 () {
	}

	@Test
	operation test14 () {
	    assert !active (Sensor_Component._Sensor_Component_._off_)
	}

	@Test
	operation test15 () {
	    assert !active (Sensor_Component._Sensor_Component_._Sensor_Monitoring_._Sensing_.Threshold_Reached)
	}

	@Test
	operation test16 () {
	    assert !active (Sensor_Component._Sensor_Component_._Sensor_Monitoring_)
	}

	@Test
	operation test17 () {
	    assert !active (Sensor_Component._Sensor_Component_._Sensor_Monitoring_._Sensing_.Under_Threshold)
	}

	@Test
	operation test18 () {
	    raise noActivitySensed
	    assert !is_active
	}

	@Test
	operation test19 () {
	    enter
	    assert is_active
	}

	@Test
	operation test20 () {
	    assert !is_active
	}

	@Test
	operation test21 () {
	    exit
	    assert !is_final
	}

	@Test
	operation test27 () {
	    assert !is_active
	    assert !is_final
	}

	@Test
	operation test28 () {
	    assert !is_active
	    enter
	    raise off
	    raise on
	    assert is_active
	}

	@Test
	operation test29 () {
	    assert !is_active
	    enter
	    proceed 1s
	    assert is_active
	}

}