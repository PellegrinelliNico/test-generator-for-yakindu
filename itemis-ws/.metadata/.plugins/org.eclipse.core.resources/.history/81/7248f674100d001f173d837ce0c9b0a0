testclass eventdrivenTest for statechart eventdriven {
	@Test
	operation test04 () {
	    enter
	    raise input
	    raise input
	    assert is_active
	}

	@Test
	operation test05 () {
	    enter
	    proceed 500ms
	    assert is_active
	}

	@Test
	operation test06 () {
	    exit
	    assert !is_final
	}

	@Test
	operation test07 () {
	    enter
	    raise input
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test08 () {
	    enter
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test09 () {
	    enter
	    raise input
	    proceed 200ms
	    assert is_active
	    assert active (eventdriven.main_region.StateC)
	}

	@Test
	operation test10 () {
	}

	@Test
	operation test11 () {
	    assert !active (eventdriven.main_region.StateE)
	}

	@Test
	operation test12 () {
	    assert !active (eventdriven.main_region.StateD)
	}

	@Test
	operation test13 () {
	    assert !active (eventdriven.main_region.StateB)
	}

	@Test
	operation test14 () {
	    assert !active (eventdriven.main_region.StateC)
	}

	@Test
	operation test15 () {
	    assert !active (eventdriven.main_region.StateA)
	}

	@Test
	operation test16 () {
	    triggerWithoutEvent
	    assert !is_active
	}

	@Test
	operation test17 () {
	    assert !is_active
	    enter
	    raise input
	    proceed 200ms
	    triggerWithoutEvent
	    assert is_active
	}

	@Test
	operation test18 () {
	    assert !is_active
	}

	@Test
	operation test21 () {
	    assert !is_active
	    assert !is_final
	}

	@Test
	operation test22 () {
	    enter
	    raise input
	    proceed 200ms
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test23 () {
	    assert !is_active
	    enter
	    assert is_active
	}

}