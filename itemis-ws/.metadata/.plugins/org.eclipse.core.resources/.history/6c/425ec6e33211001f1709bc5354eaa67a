	@Test
	operation test05 () {
	    enter
	    raise sensor.motion
	    raise sensor.motion
	    assert is_active
	}

	@Test
	operation test06 () {
	    enter
	    raise sensor.motion
	    proceed 30000000000ns
	    assert is_active
	}

	@Test
	operation test07 () {
	    enter
	    triggerWithoutEvent
	    assert is_active
	}

	@Test
	operation test08 () {
	    enter
	    raise user.on_button
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test09 () {
	    enter
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
