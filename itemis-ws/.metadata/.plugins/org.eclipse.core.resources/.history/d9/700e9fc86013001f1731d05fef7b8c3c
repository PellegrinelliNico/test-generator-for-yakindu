testclass Statechart3Test for statechart Statechart3 {
	@Test
	operation test00 () {
	    triggerWithoutEvent
	    assert !is_active
	}

	@Test
	operation test04 () {
	}

	@Test
	operation test05 () {
	    enter
	    raise pir.OFF
	    assert is_active
	}

	@Test
	operation test06 () {
	    enter
	    raise pir.OFF
	    raise pir.OFF
	    assert is_active
	}

	@Test
	operation test07 () {
	    exit
	    assert !is_final
	}

	@Test
	operation test08 () {
	    enter
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test09 () {
	    enter
	    raise pir.OFF
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test10 () {
	    assert !is_active
	    enter
	    assert active (Statechart3.main_region.Decision)
	}

	@Test
	operation test11 () {
	    assert !is_active
	    enter
	    raise pir.ON
	    assert active (Statechart3.main_region.StateLightOn)
	}

	@Test
	operation test12 () {
	    assert !is_active
	    enter
	    raise pir.OFF
	    assert active (Statechart3.main_region.StateLightOff)
	}

	@Test
	operation test13 () {
	    assert !is_active
	}

	@Test
	operation test14 () {
	    assert !is_active
	    assert !active (Statechart3.main_region.Decision)
	}

	@Test
	operation test15 () {
	    assert !is_active
	    assert !active (Statechart3.main_region.StateLightOn)
	}

	@Test
	operation test16 () {
	    assert !active (Statechart3.main_region.StateLightOff)
	    assert !is_active
	}

	@Test
	operation test17 () {
	    assert !is_active
	    enter
	    raise pir.ON
	    raise pir.ON
	    assert is_active
	}

	@Test
	operation test18 () {
	    assert !is_active
	}

	@Test
	operation test20 () {
	    assert !is_final
	    assert !is_active
	}

	@Test
	operation test22 () {
	    enter
	    raise pir.ON
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test23 () {
	    assert !is_active
	}

	@Test
	operation test24 () {
	    assert !is_active
	    enter
	    raise pir.OFF
	    proceed 1s
	    assert is_active
	}

	@Test
	operation test26 () {
	    assert !is_active
	    enter
	    assert is_active
	}

}