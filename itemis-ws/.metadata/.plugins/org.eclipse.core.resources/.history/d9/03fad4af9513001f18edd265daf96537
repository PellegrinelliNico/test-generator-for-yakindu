testclass Elevator_UnitTest for statechart Elevator_Unit {
	@Test
	operation test00 () {
	    triggerWithoutEvent
	}

	@Test
	operation test01 () {
	    proceed 100ms
	}

	@Test
	operation test02 () {
	    raise stopped
	}

	@Test
	operation test06 () {
	    raise toggle
	}

	@Test
	operation test07 () {
	}

	@Test
	operation test08 () {
	    enter
	    raise toggle
	    raise input.down
	}

	@Test
	operation test09 () {
	    enter
	    raise toggle
	    raise input.up
	}

	@Test
	operation test10 () {
	    enter
	    raise toggle
	    raise direction.up
	    raise input.up
	}

	@Test
	operation test11 () {
	    enter
	    raise toggle
	    raise input.up
	}

	@Test
	operation test12 () {
	    enter
	    raise toggle
	    raise direction.up
	    exit
	    assert !is_active
	}

	@Test
	operation test13 () {
	    enter
	    raise toggle
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test14 () {
	    enter
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test15 () {
	    enter
	    raise toggle
	    assert is_active
	    assert active (Elevator_Unit.main_region._Elevator_Available_.input._gettingInput_)
	}

	@Test
	operation test16 () {
	    enter
	    raise toggle
	    assert is_active
	    assert active (Elevator_Unit.main_region._Elevator_Available_.level._ElevatorLevel_)
	}

	@Test
	operation test17 () {
	    enter
	    raise toggle
	    assert is_active
	    assert active (Elevator_Unit.main_region._Elevator_Available_.movement._standby_)
	}

	@Test
	operation test18 () {
	    enter
	    raise toggle
	    assert is_active
	    assert active (Elevator_Unit.main_region._Elevator_Available_)
	}

	@Test
	operation test19 () {
	    enter
	    assert is_active
	    assert !active (Elevator_Unit.main_region._Elevator_Available_)
	}

	@Test
	operation test20 () {
	    enter
	    assert is_active
	    assert active (Elevator_Unit.main_region._Elevator_Unavailable_)
	}

	@Test
	operation test21 () {
	    assert !active (Elevator_Unit.main_region._Elevator_Available_.level._ElevatorLevel_)
	}

	@Test
	operation test22 () {
	    assert !active (Elevator_Unit.main_region._Elevator_Available_)
	}

	@Test
	operation test23 () {
	    assert !active (Elevator_Unit.main_region._Elevator_Available_.input._submitInput_)
	}

	@Test
	operation test24 () {
	    assert !active (Elevator_Unit.main_region._Elevator_Available_.input._gettingInput_)
	}

	@Test
	operation test25 () {
	    assert !active (Elevator_Unit.main_region._Elevator_Available_.level._ElevatorLevel_.r1._readingDataFromElevator_)
	}

	@Test
	operation test26 () {
	    assert !active (Elevator_Unit.main_region._Elevator_Available_.movement._standby_)
	}

	@Test
	operation test27 () {
	    assert !active (Elevator_Unit.main_region._Elevator_Available_.movement._downwards_)
	}

	@Test
	operation test28 () {
	    assert !active (Elevator_Unit.main_region._Elevator_Available_.movement._upwards_)
	}

	@Test
	operation test29 () {
	    assert !active (Elevator_Unit.main_region._Elevator_Available_.activity._active_)
	}

	@Test
	operation test30 () {
	    assert !active (Elevator_Unit.main_region._Elevator_Available_.activity._idle_)
	}

	@Test
	operation test31 () {
	}

	@Test
	operation test32 () {
	    assert !active (Elevator_Unit.main_region._Elevator_Unavailable_)
	}

	@Test
	operation test33 () {
	    raise input.submit
	}

	@Test
	operation test34 () {
	    assert !is_active
	}

	@Test
	operation test35 () {
	    enter
	    assert is_active
	}

	@Test
	operation test36 () {
	    exit
	    assert !is_active
	}

	@Test
	operation test38 () {
	    assert !is_active
	}

	@Test
	operation test39 () {
	    enter
	    raise toggle
	    raise inUse
	    assert is_active
	}

	@Test
	operation test44 () {
	    enter
	    raise toggle
	    raise input.submit
	    assert is_active
	}

	@Test
	operation test45 () {
	    assert !is_active
	    enter
	    raise toggle
	    raise direction.down
	    raise direction.up
	    assert is_active
	}

	@Test
	operation test46 () {
	    assert !is_active
	    assert !is_final
	}

	@Test
	operation test47 () {
	    assert !is_active
	    enter
	    raise toggle
	    raise direction.up
	    raise direction.down
	    assert is_active
	}

	@Test
	operation test48 () {
	    assert !is_active
	    enter
	    raise toggle
	    raise direction.down
	    raise direction.down
	    assert is_active
	}

	@Test
	operation test50 () {
	    assert !is_active
	    enter
	    raise toggle
	    raise toggle
	    assert is_active
	}

}