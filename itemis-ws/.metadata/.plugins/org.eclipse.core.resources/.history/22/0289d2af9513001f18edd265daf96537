testclass Controller_ComponentTest for statechart Controller_Component {
	@Test
	operation test00 () {
	    raise Actuator.trigger
	}

	@Test
	operation test01 () {
	    triggerWithoutEvent
	    assert !is_final
	}

	@Test
	operation test06 () {
	}

	@Test
	operation test07 () {
	    enter
	    raise off
	    raise off
	    assert is_active
	}

	@Test
	operation test08 () {
	    enter
	    raise on
	    assert is_active
	}

	@Test
	operation test09 () {
	    enter
	    proceed 100ms
	    assert is_active
	}

	@Test
	operation test10 () {
	    enter
	    raise off
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test11 () {
	    enter
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test12 () {
	    enter
	    raise off
	    assert is_active
	    assert active (Controller_Component._Controller_Component_._off_)
	}

	@Test
	operation test13 () {
	    enter
	    assert is_active
	    assert active (Controller_Component._Controller_Component_._Controller_isON_._Controller_Receiving_and_Sending_Signals_._Reading_Sensor_Data_)
	}

	@Test
	operation test14 () {
	    enter
	    assert is_active
	    assert active (Controller_Component._Controller_Component_._Controller_isON_)
	}

	@Test
	operation test15 () {
	}

	@Test
	operation test16 () {
	    assert !active (Controller_Component._Controller_Component_._off_)
	}

	@Test
	operation test17 () {
	    assert !active (Controller_Component._Controller_Component_._Controller_isON_._Controller_Receiving_and_Sending_Signals_._Trigger_Actuator_)
	}

	@Test
	operation test18 () {
	    assert !active (Controller_Component._Controller_Component_._Controller_isON_._Controller_Receiving_and_Sending_Signals_._Controller_Processing_)
	}

	@Test
	operation test19 () {
	    assert !active (Controller_Component._Controller_Component_._Controller_isON_)
	}

	@Test
	operation test20 () {
	    assert !active (Controller_Component._Controller_Component_._Controller_isON_._Controller_Receiving_and_Sending_Signals_._Reading_Sensor_Data_)
	}

	@Test
	operation test21 () {
	    assert !is_active
	}

	@Test
	operation test22 () {
	    exit
	    assert !is_active
	}

	@Test
	operation test25 () {
	    assert !is_active
	}

	@Test
	operation test26 () {
	    assert !is_active
	}

	@Test
	operation test27 () {
	    assert !is_active
	    assert !is_final
	}

	@Test
	operation test28 () {
	    assert !is_active
	    enter
	    raise off
	    raise on
	    assert is_active
	}

	@Test
	operation test31 () {
	    assert !is_active
	    enter
	    assert is_active
	}

}