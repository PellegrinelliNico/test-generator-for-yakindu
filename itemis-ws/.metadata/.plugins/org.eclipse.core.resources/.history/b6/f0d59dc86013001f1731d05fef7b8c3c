testclass MqttClientStatechartTest for statechart MqttClientStatechart {
	@Test
	operation test01 () {
	}

	@Test
	operation test02 () {
	    enter
	    proceed 1 cycle
	    assert is_active
	}

	@Test
	operation test03 () {
	    enter
	    exit
	    assert !is_active
	}

	@Test
	operation test04 () {
	    enter
	    assert is_active
	}

	@Test
	operation test05 () {
	    enter
	    assert !active (MqttClientStatechart.main_region.Online)
	}

	@Test
	operation test06 () {
	    enter
	    assert is_active
	    assert active (MqttClientStatechart.main_region.Undefined.r1.Entry)
	}

	@Test
	operation test07 () {
	    enter
	    assert active (MqttClientStatechart.main_region.Undefined)
	}

	@Test
	operation test08 () {
	}

	@Test
	operation test09 () {
	    assert !active (MqttClientStatechart.main_region.Offline)
	}

	@Test
	operation test10 () {
	    assert !active (MqttClientStatechart.main_region.Online.r1.PublishMessages)
	}

	@Test
	operation test11 () {
	    assert !active (MqttClientStatechart.main_region.Online)
	}

	@Test
	operation test12 () {
	    assert !active (MqttClientStatechart.main_region.Offline.r1.ConnectToAccesspoint)
	}

	@Test
	operation test13 () {
	    assert !active (MqttClientStatechart.main_region.Offline.r1.EnableDhcp)
	}

	@Test
	operation test14 () {
	    assert !active (MqttClientStatechart.main_region.Offline.r1.ConfigureMqtt)
	}

	@Test
	operation test15 () {
	    assert !active (MqttClientStatechart.main_region.Offline.r1.SetStationMode)
	}

	@Test
	operation test16 () {
	    assert !active (MqttClientStatechart.main_region.Offline.r1.Entry)
	}

	@Test
	operation test17 () {
	    assert !active (MqttClientStatechart.main_region.Offline.r1.SetTopic)
	}

	@Test
	operation test18 () {
	    assert !active (MqttClientStatechart.main_region.Online.r1.ReceiveMessages)
	}

	@Test
	operation test19 () {
	    assert !active (MqttClientStatechart.main_region.Online.r1.ConnectToBroker)
	}

	@Test
	operation test20 () {
	    assert !active (MqttClientStatechart.main_region.Online.r1.PublishMessages.r1.Wait)
	}

	@Test
	operation test21 () {
	    assert !active (MqttClientStatechart.main_region.Online.r1.PublishMessages.r1.PublishTopic)
	}

	@Test
	operation test22 () {
	    assert !active (MqttClientStatechart.main_region.Online.r1.Entry)
	}

	@Test
	operation test23 () {
	    assert !active (MqttClientStatechart.main_region.Undefined.r1.Stop)
	}

	@Test
	operation test24 () {
	    assert !active (MqttClientStatechart.main_region.Undefined.r1.Start)
	}

	@Test
	operation test25 () {
	    assert !active (MqttClientStatechart.main_region.Undefined.r1.ResetSystem)
	}

	@Test
	operation test26 () {
	    assert !active (MqttClientStatechart.main_region.Undefined.r1.RestartModule)
	}

	@Test
	operation test27 () {
	    assert !active (MqttClientStatechart.main_region.Undefined.r1.TestModule)
	}

	@Test
	operation test28 () {
	    assert !active (MqttClientStatechart.main_region.Undefined.r1.ResetModule)
	}

	@Test
	operation test29 () {
	    assert !active (MqttClientStatechart.main_region.Undefined)
	}

	@Test
	operation test30 () {
	    assert !active (MqttClientStatechart.main_region.Undefined.r1.Entry)
	}

	@Test
	operation test31 () {
	    proceed 1 cycle
	    assert !is_final
	}

	@Test
	operation test32 () {
	    assert !is_active
	    enter
	    assert is_active
	}

	@Test
	operation test33 () {
	    assert !is_active
	}

	@Test
	operation test34 () {
	    exit
	    assert !is_final
	}

	@Test
	operation test37 () {
	    raise WizFi360.ready
	}

	@Test
	operation test38 () {
	    raise WizFi360.fail
	}

	@Test
	operation test39 () {
	}

	@Test
	operation test40 () {
	    raise WizFi360.ok
	}

	@Test
	operation test41 () {
	    assert !is_final
	    assert !is_active
	}

	@Test
	operation test42 () {
	    assert !is_active
	}

	@Test
	operation test43 () {
	    assert !is_active
	}

	@Test
	operation test44 () {
	    enter
	    proceed 1s
	    proceed 1 cycle
	    proceed 1 cycle
	    proceed 1 cycle
	    assert is_active
	}

}