	@Test
	operation test02 () {
	    enter
	    raise Panel.btn_pressed
	    raise Panel.btn_pressed
	    raise Panel.btn_pressed
	    raise Panel.btn_pressed
	    raise Panel.btn_pressed
	    triggerWithoutEvent
	    assert is_active
	}

	@Test
	operation test03 () {
	    enter
	    triggerWithoutEvent
	    assert is_active
	}

	@Test
