	    enter
	    raise Panel.btn_pressed
	    raise Panel.btn_pressed
	    raise Panel.btn_pressed
	    raise Panel.btn_pressed
	    raise Panel.btn_pressed
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
