package implementation;

import static org.junit.Assert.*;

import org.junit.Test;

public class EqualStatechartsTest {

	@Test
	public void test() {
		Statechart stc = new Statechart();
		stc.enter();
		assertTrue(stc.isStateActive(
				Statechart.State.MAIN_REGION_STATE_A_REGION_1_STATE_B_REGION_2_STATE_C));
	}

}
