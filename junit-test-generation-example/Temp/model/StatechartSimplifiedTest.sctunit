	    assert !is_active
	}

	@Test
	operation test32 () {
	    raise power_on
	    enter
	    proceed 1 cycle
	    raise work
	    proceed 1 cycle
	    raise end_all
	    proceed 1 cycle
	    assert is_active
	}

	@Test
	operation test33 () {
	    raise power_on
	    enter
	    proceed 1 cycle
	    raise analize
	    proceed 1 cycle
	    raise end_all
	    proceed 1 cycle
