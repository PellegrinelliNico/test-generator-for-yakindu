	    assert !active (Statechart.mainregion.on)
	}

	@Test
	operation test11 () {
	    enter
	    raise power_on
	    proceed 1 cycle
	    assert is_active
	    assert active (Statechart.mainregion.idle)
	}

	@Test
	operation test12 () {
	    enter
	    assert is_active
	    assert active (Statechart.mainregion.off)
	}

	@Test
	operation test28 () {
	    enter
	    assert is_active
	}

	@Test
	operation test29 () {
	    raise power_on
	    enter
	    proceed 1 cycle
