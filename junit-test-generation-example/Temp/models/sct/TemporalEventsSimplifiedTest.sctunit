testclass TemporalEventsTest for statechart TemporalEvents {
	@Test
	operation test04 () {
	    enter
	    proceed 2s
	    raise stop
	    proceed 2s
	    assert is_active
	}

	@Test
	operation test05 () {
	    enter
	    proceed 2s
	    raise next
	    proceed 2s
	    assert is_active
	}

	@Test
	operation test06 () {
	    enter
	    proceed 2s
	    raise check
	    assert is_active
	}

	@Test
	operation test07 () {
	    enter
	    triggerWithoutEvent
	    assert is_active
	}

	@Test
	operation test08 () {
	    exit
	    assert !is_active
	}

	@Test
	operation test09 () {
	    enter
	    proceed 2s
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test10 () {
	    enter
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test11 () {
	    enter
	    proceed 2s
	    raise stop
	    assert is_final
	    assert active (TemporalEvents.main_region._final_)
	}

	@Test
	operation test12 () {
	    enter
	    proceed 2s
	    assert is_active
	    assert active (TemporalEvents.main_region.StateB)
	}

	@Test
	operation test13 () {
	    enter
	    assert is_active
	    assert active (TemporalEvents.main_region.StateA)
	}

	@Test
	operation test14 () {
	}

	@Test
	operation test15 () {
	    assert !active (TemporalEvents.main_region._final_)
	}

	@Test
	operation test16 () {
	    assert !active (TemporalEvents.main_region.StateB)
	}

	@Test
	operation test17 () {
	    assert !active (TemporalEvents.main_region.ERR)
	}

	@Test
	operation test18 () {
	    assert !active (TemporalEvents.main_region.StateA)
	}

	@Test
	operation test20 () {
	    triggerWithoutEvent
	    assert !is_active
	}

	@Test
	operation test21 () {
	    enter
	    proceed 2s
	    raise stop
	    assert is_final
	}

	@Test
	operation test22 () {
	    assert !is_final
	    assert !is_active
	}

	@Test
	operation test23 () {
	    assert !is_active
	    enter
	    assert is_active
	}

	@Test
	operation test24 () {
	    assert !is_active
	}

	@Test
	operation test28 () {
	    enter
	    proceed 2s
	    raise next
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test29 () {
	    assert !is_active
	    enter
	    proceed 2s
	    raise check
	    assert is_active
	}

	@Test
	operation test31 () {
	    enter
	    proceed 2s
	    raise stop
	    assert is_active
	    exit
	    assert !is_active
	}

}