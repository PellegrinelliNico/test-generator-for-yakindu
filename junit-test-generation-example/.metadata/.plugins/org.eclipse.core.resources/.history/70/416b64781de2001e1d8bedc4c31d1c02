testclass OperationsTest for statechart Operations {
	@Test
	operation test00 () {
	}

	@Test
	operation test01 () {
	}

	@Test
	operation test02 () {
	}

	@Test
	operation test03 () {
	    assert !active (Operations.main_region.StateB)
	}

	@Test
	operation test04 () {
	    assert !active (Operations.main_region.StateA)
	}

	@Test
	operation test05 () {
	    assert !is_active
	}

	@Test
	operation test06 () {
	    enter
	}

	@Test
	operation test07 () {
	    assert !is_final
	}

	@Test
	operation test08 () {
	    raise I.b
	}

	@Test
	operation test09 () {
	    exit
	    assert !is_final
	}

	@Test
	operation test10 () {
	    raise a
	}

	@Test
	operation test11 () {
	    triggerWithoutEvent
	}

}