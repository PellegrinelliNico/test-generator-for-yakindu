testclass StatechartTest for statechart Statechart {

	@Test
	operation test03 () {
	    enter
	    raise power_on
	    raise analize
	    raise start
	    assert is_active
	}

	@Test
	operation test10 () {
	    raise start
	    enter
	    raise power_on
	    raise analize
	    raise start
	    assert is_active
	}

	@Test
	operation test11 () {
	    enter
	    raise power_on
	    raise work
	    raise analize
	    assert !is_final
	}

	@Test
	operation test12 () {
	    enter
	    raise power_on
	    triggerWithoutEvent
	    assert is_active
	}

	@Test
	operation test13 () {
	    enter
	    raise end_all
	    assert is_active
	}

	@Test
	operation test14 () {
	    enter
	    raise power_on
	    raise analize
	    raise start
	    raise end_all
	    assert is_active
	}

	@Test
	operation test16 () {
	    enter
	    raise power_on
	    raise analize
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test17 () {
	    enter
	    raise power_on
	    raise work
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test18 () {
	    enter
	    raise power_on
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test19 () {
	    enter
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test20 () {
	    enter
	    raise power_on
	    raise analize
	    raise start
	    assert is_active
	    assert active (Statechart.mainregion.on.r1.analyses.r2.an2a)
	}

	@Test
	operation test21 () {
	    enter
	    raise power_on
	    raise analize
	    raise start
	    assert is_active
	    assert active (Statechart.mainregion.on.r1.analyses.r1.an1)
	}

	@Test
	operation test22 () {
	    enter
	    raise power_on
	    raise analize
	    raise start
	    assert is_active
	    assert active (Statechart.mainregion.on.r1.analyses)
	}

	@Test
	operation test23 () {
	    enter
	    assert is_active
	    assert !active (Statechart.mainregion.on.r1.analyses)
	}

	@Test
	operation test24 () {
	    enter
	    raise power_on
	    raise analize
	    assert is_active
	    assert active (Statechart.mainregion.on)
	}

	@Test
	operation test25 () {
	    enter
	    assert is_active
	    assert !active (Statechart.mainregion.on)
	}

	@Test
	operation test26 () {
	    enter
	    raise power_on
	    assert is_active
	    assert active (Statechart.mainregion.idle)
	}

	@Test
	operation test27 () {
	    enter
	    assert is_active
	    assert active (Statechart.mainregion.off)
	}

	@Test
	operation test41 () {
	    enter
	    raise power_on
	    raise work
	    raise end_all
	    assert is_final
	}

	@Test
	operation test44 () {
	    enter
	    assert is_active
	}

	@Test
	operation test47 () {
	    enter
	    raise power_on
	    raise analize
	    raise end_all
	    exit
	    assert !is_active
	}

	@Test
	operation test48 () {
	    enter
	    raise power_on
	    raise analize
	    raise start
	    raise a
	    assert is_active
	}

	@Test
	operation test49 () {
	    enter
	    raise power_on
	    raise work
	    raise end_all
	    raise start
	    assert is_active
	}

	@Test
	operation test51 () {
	    enter
	    raise power_on
	    raise analize
	    raise start
	    exit
	    assert !is_active
	}

}

