testclass ExamplStcTest for statechart ExamplStc {
	@Test
	operation test00 () {
	    enter
	    raise power_on
	    raise Work
	    raise end
	    assert is_final
	    assert active (ExamplStc.main_region.On)
	}

	@Test
	operation test02 () {
	    enter
	    raise power_on
	    exit
	    raise start
	    exit
	    assert is_active
	    assert active (ExamplStc.main_region.On.r_0.Simplified_analyses012.r_2.an2_b)
	}

	@Test
	operation test04 () {
	    enter
	    raise power_on
	    exit
	    raise start
	    raise Work
	    assert is_active
	}

	@Test
	operation test05 () {
	    enter
	    raise power_on
	    raise end
	    assert is_active
	}

	@Test
	operation test06 () {
	    enter
	    triggerWithoutEvent
	    assert is_active
	}

	@Test
	operation test07 () {
	    enter
	    raise power_on
	    exit
	    raise start
	    raise end_all
	    assert is_active
	}

	@Test
	operation test08 () {
	    enter
	    raise power_on
	    exit
	    raise start
	    exit
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test10 () {
	    enter
	    raise power_on
	    exit
	    raise start
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test11 () {
	    enter
	    raise power_on
	    exit
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test12 () {
	    enter
	    raise power_on
	    raise Work
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test13 () {
	    enter
	    raise power_on
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test14 () {
	    enter
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test15 () {
	    enter
	    raise power_on
	    raise Work
	    raise end_all
	    assert is_final
	    assert active (ExamplStc.main_region._final_)
	}

	@Test
	operation test16 () {
	    enter
	    raise power_on
	    exit
	    raise start
	    raise a
	    assert is_active
	}

	@Test
	operation test17 () {
	    enter
	    raise power_on
	    exit
	    raise start
	    assert is_active
	    assert active (ExamplStc.main_region.On.r_0.Simplified_analyses012.r_2.an2_a)
	}

	@Test
	operation test19 () {
	    enter
	    raise power_on
	    exit
	    raise start
	    assert is_active
	    assert active (ExamplStc.main_region.On.r_0.Simplified_analyses012)
	}

	@Test
	operation test20 () {
	    enter
	    assert is_active
	    assert !active (ExamplStc.main_region.On.r_0.Simplified_analyses012)
	}

	@Test
	operation test21 () {
	    enter
	    raise power_on
	    exit
	    assert is_active
	    assert active (ExamplStc.main_region.On.r_0.Init_anAlyses)
	}

	@Test
	operation test22 () {
	    enter
	    raise power_on
	    raise Work
	    assert active (ExamplStc.main_region.On)
	}

	@Test
	operation test23 () {
	    enter
	    assert is_active
	    assert !active (ExamplStc.main_region.On)
	}

	@Test
	operation test24 () {
	    enter
	    raise power_on
	    assert is_active
	    assert active (ExamplStc.main_region.Simplified)
	}

	@Test
	operation test25 () {
	    enter
	    assert is_active
	    assert active (ExamplStc.main_region.Off_)
	}

	@Test
	operation test38 () {
	    enter
	    raise power_on
	    exit
	    raise start
	    exit
	    raise end_all
	    assert is_active
	}

	@Test
	operation test39 () {
	    enter
	    raise power_on
	    raise Work
	    raise end_all
	    assert is_active
	    assert is_final
	}

	@Test
	operation test41 () {
	    enter
	    raise power_on
	    raise Work
	    raise end
	    assert is_active
	    assert is_final
	}

	@Test
	operation test43 () {
	    enter
	    assert is_active
	}

	@Test
	operation test44 () {
	    enter
	    raise power_on
	    raise Work
	    raise end
	    raise end_all
	    assert is_active
	}

	@Test
	operation test46 () {
	    enter
	    raise power_on
	    exit
	    raise end_all
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test47 () {
	    enter
	    raise power_on
	    raise Work
	    raise end_all
	    exit
	    assert is_final
	}

	@Test
	operation test48 () {
	    enter
	    raise power_on
	    exit
	    raise start
	    exit
	    raise a
	    exit
	    assert !is_active
	}

}