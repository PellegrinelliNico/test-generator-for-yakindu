testclass StatechartTest for statechart Statechart {

	@Test
	operation test10 () {
	    enter
	    raise start
	    raise power_on
	    raise analize
	    raise start
	    assert is_active
	}

	@Test
	operation test11 () {
	    enter
	    raise start
	    raise power_on
	    raise analize
	    raise end_all
	    raise start
	    assert is_active
	}

	@Test
	operation test12 () {
	    enter
	    raise power_on
	    raise analize
	    raise start
	    raise b
	    raise a
	    raise end
	    raise b
	    assert is_active
	}

	@Test
	operation test13 () {
	    enter
	    raise power_on
	    raise analize
	    raise start
	    raise b
	    raise a
	    raise b
	    assert is_active
	}

	@Test
	operation test14 () {
	    enter
	    raise power_on
	    raise analize
	    raise start
	    raise b
	    enter
	    raise end_all
	    assert is_active
	}

	@Test
	operation test15 () {
	    enter
	    raise power_on
	    raise b
	    assert is_active
	}

	@Test
	operation test16 () {
	    enter
	    raise power_on
	    raise analize
	    raise start
	    raise b
	    raise end_all
	    assert is_active
	}

	@Test
	operation test17 () {
	    enter
	    raise power_on
	    raise analize
	    raise start
	    enter
	    raise end_all
	    assert is_active
	}

	@Test
	operation test19 () {
	    enter
	    raise power_on
	    raise analize
	    raise start
	    raise b
	    raise a
	    exit
	    assert !is_active
	}

	@Test
	operation test20 () {
	    enter
	    raise power_on
	    raise analize
	    raise start
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test21 () {
	    enter
	    raise power_on
	    raise analize
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test22 () {
	    enter
	    raise power_on
	    raise work
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test23 () {
	    enter
	    raise power_on
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test24 () {
	    enter
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test25 () {
	    enter
	    raise power_on
	    raise analize
	    raise end_all
	    assert is_active
	    assert active (Statechart.mainregion._final_)
	}

	@Test
	operation test26 () {
	    enter
	    raise power_on
	    raise analize
	    raise start
	    raise a
	    assert is_active
	}

	@Test
	operation test28 () {
	    enter
	    assert is_active
	    assert !active (Statechart.mainregion.on.r1.analyses)
	}

	@Test
	operation test29 () {
	    enter
	    raise power_on
	    raise analize
	    assert is_active
	    assert active (Statechart.mainregion.on.r1.initanalyses)
	}

	@Test
	operation test30 () {
	    enter
	    raise power_on
	    raise work
	    assert is_active
	    assert active (Statechart.mainregion.on.r1.working)
	}

	@Test
	operation test31 () {
	    enter
	    raise power_on
	    raise analize
	    assert is_active
	    assert active (Statechart.mainregion.on)
	}

	@Test
	operation test32 () {
	    enter
	    assert is_active
	    assert !active (Statechart.mainregion.on)
	}

	@Test
	operation test33 () {
	    enter
	    raise power_on
	    assert is_active
	    assert active (Statechart.mainregion.idle)
	}

	@Test
	operation test34 () {
	    enter
	    assert is_active
	    assert active (Statechart.mainregion.off)
	}

	@Test
	operation test47 () {
	    enter
	    raise power_on
	    raise analize
	    raise end_all
	    assert is_active
	    assert is_final
	}

	@Test
	operation test50 () {
	    enter
	    assert is_active
	}

	@Test
	operation test54 () {
	    enter
	    raise power_on
	    raise work
	    raise end_all
	    assert is_final
	    exit
	    assert !is_active
	}

	@Test
	operation test55 () {
	    enter
	    raise power_on
	    raise analize
	    raise start
	    raise b
	    raise a
	    raise end
	    raise end
	    raise b
	    assert is_final
	}

}
