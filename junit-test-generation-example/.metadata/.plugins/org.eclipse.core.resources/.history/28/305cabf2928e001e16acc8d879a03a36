testclass StatechartTest for statechart Statechart {

	@Test
	operation test17 () {
	    enter
	    raise power_on
	    raise a
	    assert is_active
	}

	@Test
	operation test18 () {
	    enter
	    raise end_all
	    assert is_active
	}

	@Test
	operation test19 () {
	    enter
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test20 () {
	    enter
	    assert is_active
	    assert !active (Statechart.mainregion.on.r1.analyses)
	}

	@Test
	operation test21 () {
	    enter
	    raise power_on
	    raise work
	    assert is_active
	    assert active (Statechart.mainregion.on)
	}

	@Test
	operation test22 () {
	    enter
	    assert is_active
	    assert !active (Statechart.mainregion.on)
	}

	@Test
	operation test23 () {
	    enter
	    raise power_on
	    assert is_active
	    assert active (Statechart.mainregion.idle)
	}

	@Test
	operation test39 () {
	    enter
	    assert is_active
	}

	@Test
	operation test42 () {
	    enter
	}

	@Test
	operation test49 () {
	    assert !is_active
	    enter
	    raise power_on
	    raise analize
	    assert is_active
	}

	@Test
	operation test51 () {
	    assert !is_active
	    enter
	    raise power_on
	    raise work
	    raise a
	    assert is_active
	}

}
