testclass StatechartTest for statechart Statechart {
	@Test
	operation test06 () {
	    raise power_on
	    enter
	    proceed 1 cycle
	    proceed 1 cycle
	    assert is_active
	}

	@Test
	operation test07 () {
	    enter
	    proceed 1 cycle
	    assert is_active
	}

	@Test
	operation test09 () {
	    raise power_on
	    enter
	    proceed 1 cycle
	    raise work
	    proceed 1 cycle
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test10 () {
	    raise power_on
	    enter
	    proceed 1 cycle
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test11 () {
	    enter
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test12 () {
	    enter
	    assert is_active
	    assert !active (Statechart.mainregion.on.r1.analyses)
	}

	@Test
	operation test13 () {
	    enter
	    assert is_active
	    assert !active (Statechart.mainregion.on)
	}

	@Test
	operation test14 () {
	    enter
	    raise power_on
	    proceed 1 cycle
	    assert is_active
	    assert active (Statechart.mainregion.idle)
	}

	@Test
	operation test15 () {
	    enter
	    assert is_active
	    assert active (Statechart.mainregion.off)
	}

	@Test
	operation test36 () {
	    raise power_on
	    assert !is_active
	    enter
	    proceed 1 cycle
	    raise analize
	    proceed 1 cycle
	    proceed 1 cycle
	    assert is_active
	}

	@Test
	operation test38 () {
	    raise power_on
	    enter
	    proceed 1 cycle
	    raise work
	    proceed 1 cycle
	    raise end_all
	    proceed 1 cycle
	    assert is_final
	    exit
	    assert !is_final
	}

	@Test
	operation test40 () {
	    assert !is_active
	    enter
	    assert is_active
	}

}