testclass ControllerTest for statechart Controller {
	@Test
	operation test03 () {
	    enter
	    raise powerOn
	    raise control2
	    raise control2
	    assert is_active
	}

	@Test
	operation test04 () {
	    enter
	    raise powerOn
	    raise control2
	    raise dec
	    assert is_active
	}

	@Test
	operation test05 () {
	    enter
	    raise powerOn
	    raise control1
	    raise dec
	    assert is_active
	}

	@Test
	operation test06 () {
	    enter
	    raise powerOn
	    raise control1
	    raise control2
	    assert is_active
	}

	@Test
	operation test07 () {
	    enter
	    raise powerOn
	    raise dec
	    assert is_active
	}

	@Test
	operation test08 () {
	    enter
	    triggerWithoutEvent
	    assert is_active
	}

	@Test
	operation test09 () {
	    enter
	    raise powerOn
	    raise control2
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test10 () {
	    enter
	    raise powerOn
	    raise control1
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test11 () {
	    enter
	    raise powerOn
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test12 () {
	    enter
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test13 () {
	    enter
	    raise powerOn
	    raise control2
	    assert is_active
	    assert active (Controller.main_region.Light_2)
	}

	@Test
	operation test14 () {
	    enter
	    raise powerOn
	    raise control1
	    assert is_active
	    assert active (Controller.main_region.Light_1)
	}

	@Test
	operation test15 () {
	    enter
	    raise powerOn
	    assert is_active
	    assert active (Controller.main_region.On)
	}

	@Test
	operation test16 () {
	    enter
	    assert is_active
	    assert active (Controller.main_region.Off)
	}

	@Test
	operation test17 () {
	}

	@Test
	operation test18 () {
	    assert !active (Controller.main_region.Light_2)
	}

	@Test
	operation test19 () {
	    assert !active (Controller.main_region.On)
	}

	@Test
	operation test20 () {
	    assert !active (Controller.main_region.Light_1)
	}

	@Test
	operation test21 () {
	    assert !active (Controller.main_region.Off)
	}

	@Test
	operation test22 () {
	    enter
	    assert is_active
	}

	@Test
	operation test23 () {
	    assert !is_active
	}

	@Test
	operation test24 () {
	    exit
	    assert !is_active
	}

	@Test
	operation test25 () {
	    raise dec
	    assert !is_final
	}

	@Test
	operation test26 () {
	    assert !is_active
	    enter
	    raise powerOn
	    raise control2
	    raise inc
	    assert is_active
	}

	@Test
	operation test27 () {
	    assert !is_active
	    enter
	    raise powerOn
	    raise control1
	    raise inc
	    assert is_active
	}

	@Test
	operation test29 () {
	    assert !is_active
	    assert !is_final
	}

}