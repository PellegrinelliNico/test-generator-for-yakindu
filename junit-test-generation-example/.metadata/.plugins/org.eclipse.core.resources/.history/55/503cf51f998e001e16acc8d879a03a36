testclass StatechartTest for statechart Statechart {

	@Test
	operation test08 () {
	    enter
	    raise power_on
	    raise analize
	    raise start
	    raise b
	    assert is_active
	    assert active (Statechart.mainregion.on.r1.analyses.r2.an2b)
	}

	@Test
	operation test09 () {
	    enter
	    raise power_on
	    raise analize
	    raise start
	    assert is_active
	    assert active (Statechart.mainregion.on.r1.analyses.r1.an1)
	}

	@Test
	operation test12 () {
	    enter
	    raise power_on
	    raise analize
	    raise start
	    raise b
	    raise analize
	    assert !is_final
	}

	@Test
	operation test13 () {
	    enter
	    raise power_on
	    raise analize
	    raise analize
	    assert is_active
	}

	@Test
	operation test14 () {
	    enter
	    raise power_on
	    raise analize
	    raise start
	    raise analize
	    assert is_active
	}

	@Test
	operation test15 () {
	    enter
	    raise power_on
	    raise work
	    raise analize
	    assert is_active
	}

	@Test
	operation test16 () {
	    enter
	    raise power_on
	    raise analize
	    raise start
	    raise b
	    raise a
	    raise analize
	    assert is_active
	}

	@Test
	operation test17 () {
	    enter
	    raise power_on
	    raise analize
	    raise end_all
	    raise analize
	    assert is_active
	}

	@Test
	operation test18 () {
	    enter
	    raise power_on
	    raise start
	    raise analize
	    raise start
	    assert is_active
	}

	@Test
	operation test19 () {
	    enter
	    raise power_on
	    raise analize
	    raise start
	    raise b
	    raise end_all
	    assert is_active
	}

	@Test
	operation test20 () {
	    enter
	    raise power_on
	    raise analize
	    raise start
	    enter
	    raise end_all
	    assert is_active
	}

	@Test
	operation test21 () {
	    enter
	    raise power_on
	    raise analize
	    raise start
	    raise b
	    exit
	    assert !is_active
	}

	@Test
	operation test23 () {
	    enter
	    raise power_on
	    raise analize
	    raise start
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test24 () {
	    enter
	    raise power_on
	    raise analize
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test25 () {
	    enter
	    raise power_on
	    raise work
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test26 () {
	    enter
	    raise power_on
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test27 () {
	    enter
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test28 () {
	    enter
	    raise power_on
	    raise analize
	    raise end_all
	    assert is_active
	    assert active (Statechart.mainregion._final_)
	}

	@Test
	operation test29 () {
	    enter
	    raise power_on
	    raise analize
	    raise start
	    raise a
	    assert !is_final
	}

	@Test
	operation test30 () {
	    enter
	    raise power_on
	    raise analize
	    raise start
	    assert is_active
	    assert active (Statechart.mainregion.on.r1.analyses.r2.an2a)
	}

	@Test
	operation test31 () {
	    enter
	    raise power_on
	    raise analize
	    raise start
	    raise b
	    enter
	    raise power_on
	    assert is_active
	}

	@Test
	operation test32 () {
	    enter
	    raise power_on
	    raise analize
	    raise start
	    assert is_active
	    assert active (Statechart.mainregion.on.r1.analyses)
	}

	@Test
	operation test33 () {
	    enter
	    assert is_active
	    assert !active (Statechart.mainregion.on.r1.analyses)
	}

	@Test
	operation test34 () {
	    enter
	    raise power_on
	    raise analize
	    assert is_active
	    assert active (Statechart.mainregion.on.r1.initanalyses)
	}

	@Test
	operation test35 () {
	    enter
	    raise power_on
	    raise work
	    assert is_active
	    assert active (Statechart.mainregion.on.r1.working)
	}

	@Test
	operation test36 () {
	    enter
	    raise power_on
	    raise analize
	    assert is_active
	    assert active (Statechart.mainregion.on)
	}

	@Test
	operation test37 () {
	    enter
	    assert is_active
	    assert !active (Statechart.mainregion.on)
	}

	@Test
	operation test38 () {
	    enter
	    raise power_on
	    assert is_active
	    assert active (Statechart.mainregion.idle)
	}

	@Test
	operation test39 () {
	    enter
	    assert is_active
	    assert active (Statechart.mainregion.off)
	}

	@Test
	operation test53 () {
	    enter
	    raise power_on
	    raise analize
	    raise end_all
	    assert is_active
	    assert is_final
	}

	@Test
	operation test56 () {
	    enter
	    assert is_active
	}

	@Test
	operation test58 () {
	    enter
	    raise power_on
	    raise analize
	    raise end_all
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test59 () {
	    enter
	    raise power_on
	    raise work
	    raise end_all
	    assert is_active
	}

	@Test
	operation test62 () {
	    enter
	    raise power_on
	    raise analize
	    raise start
	    raise b
	    raise a
	    raise end_all
	    assert is_final
	}

}



