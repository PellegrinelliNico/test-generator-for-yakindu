testclass LightSwitchTest for statechart LightSwitch {

	@Test
	operation test00 () {
	    enter
	    raise power_on
	    raise analize
	    assert active (Statechart.main.region.on)
	    assert active (Statechart.main.region.on)
	}

	@Test
	operation test06 () {
	    enter
	    raise power_on
	    raise analize
	}

	@Test
	operation test07 () {
	    enter
	    raise power_on
	    raise end
	}

	@Test
	operation test08 () {
	    enter
	    raise analize
	}

	@Test
	operation test10 () {
	    enter
	    raise power_on
	    raise analize
	    exit
	}

	@Test
	operation test11 () {
	    enter
	    raise power_on
	    raise work
	    exit
	}

	@Test
	operation test12 () {
	    enter
	    raise power_on
	    exit
	}

	@Test
	operation test13 () {
	    enter
	    exit
	}

	@Test
	operation test14 () {
	    enter
	    raise power_on
	    raise analize
	    assert active (Statechart.main.region.on.r1.analyzing)
	    assert active (Statechart.main.region.on.r1.analyzing)
	}

	@Test
	operation test15 () {
	    enter
	    raise power_on
	    raise work
	    assert active (Statechart.main.region.on.r1.working)
	    assert active (Statechart.main.region.on.r1.working)
	}

	@Test
	operation test16 () {
	    enter
	    raise power_on
	    raise work
	    assert active (Statechart.main.region.on)
	    assert active (Statechart.main.region.on)
	}

	@Test
	operation test17 () {
	    enter
	    assert active (Statechart.main.region.on)
	    assert !active (Statechart.main.region.on)
	}

	@Test
	operation test18 () {
	    enter
	    raise power_on
	    assert active (Statechart.main.region.idle)
	    assert active (Statechart.main.region.idle)
	}

	@Test
	operation test19 () {
	    enter
	    assert active (Statechart.main.region.off)
	    assert active (Statechart.main.region.off)
	}

	@Test
	operation test28 () {
	    enter
	    raise power_on
	    raise analize
	    raise end
	}

	@Test
	operation test30 () {
	    enter
	}

	@Test
	operation test33 () {
	    enter
	}

	@Test
	operation test34 () {
	    enter
	    raise power_on
	    raise analize
	    raise end
	}

	@Test
	operation test35 () {
	    enter
	    raise power_on
	    raise analize
	    raise end
	    exit
	}

	@Test
	operation test36 () {
	    enter
	    raise power_on
	    raise work
	    raise end
	}

}