testclass HistoryTest for statechart History {
	@Test
	operation test00 () {
	    enter
	    raise next
	    raise next
	    assert is_active
	    assert active (History.main_region.Answering_Questions.r1.q2)
	}

	@Test
	operation test01 () {
	    enter
	    raise start
	    raise next
	    raise next
	    assert is_active
	    assert active (History.main_region.Answering_Questions)
	}

	@Test
	operation test02 () {
	}

	@Test
	operation test03 () {
	    triggerWithoutEvent
	    assert !is_final
	}

	@Test
	operation test04 () {
	    enter
	    triggerWithoutEvent
	    assert is_active
	}

	@Test
	operation test05 () {
	    enter
	    raise start
	    assert is_active
	}

	@Test
	operation test06 () {
	    exit
	    assert !is_active
	}

	@Test
	operation test07 () {
	    enter
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test08 () {
	    enter
	    raise start
	    raise next
	    raise next
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test09 () {
	    enter
	    raise next
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test10 () {
	    enter
