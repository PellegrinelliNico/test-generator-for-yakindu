	    raise next
	    assert is_final
	}

	@Test
	operation test36 () {
	    assert !is_active
	    assert !is_final
	}

	@Test
	operation test37 () {
	    assert !is_active
	    enter
	    assert is_active
	}

	@Test
	operation test38 () {
	    assert !is_active
	}

	@Test
	operation test39 () {
	    enter
