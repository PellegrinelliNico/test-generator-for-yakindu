testclass SimpleChoiceTest for statechart SimpleChoice {
	@Test
	operation test00 () {
	}

	@Test
	operation test01 () {
	}

	@Test
	operation test02 () {
	}

	@Test
	operation test03 () {
	}

	@Test
	operation test04 () {
	}

	@Test
	operation test05 () {
	    enter
	    raise event0
	    triggerWithoutEvent
	    assert is_active
	}

	@Test
	operation test06 () {
	    enter
	    raise event0
	    assert is_active
	}

	@Test
	operation test07 () {
	    enter
	    raise event0
	    assert is_active
	}

	@Test
	operation test08 () {
	    enter
	    triggerWithoutEvent
	    assert is_active
	}

	@Test
	operation test09 () {
	    assert !is_active
	    exit
	}

	@Test
	operation test10 () {
	    enter
	    raise event0
	    exit
	}

	@Test
	operation test11 () {
	    enter
	    raise event0
	    assert is_active
	    exit
	}

	@Test
	operation test12 () {
	    enter
	    raise event0
	    exit
	}

	@Test
	operation test13 () {
	    enter
	    assert is_active
	    exit
	}

	@Test
	operation test14 () {
	    enter
	    raise event0
	    raise event0
	    assert is_final
	    assert active (SimpleChoice.main_region._final_)
	}

	@Test
	operation test15 () {
	    assert !is_active
	    enter
	    raise event0
	    assert active (SimpleChoice.main_region.State2)
	}

	@Test
	operation test16 () {
	    enter
	    raise event0
	    assert active (SimpleChoice.main_region.State1)
	}

	@Test
	operation test17 () {
	    assert !is_active
	    enter
	    assert active (SimpleChoice.main_region.State0)
	}

	@Test
	operation test18 () {
	    assert !is_active
	}

	@Test
	operation test19 () {
	    assert !active (SimpleChoice.main_region.State3)
	    assert !is_active
	}

	@Test
	operation test20 () {
	    assert !is_active
	    assert !active (SimpleChoice.main_region.State2)
	}

	@Test
	operation test21 () {
	    assert !active (SimpleChoice.main_region.State1)
	    assert !is_active
	}

	@Test
	operation test22 () {
	    assert !is_active
	    assert !active (SimpleChoice.main_region._final_)
	}

	@Test
	operation test23 () {
	    assert !is_active
	    assert !active (SimpleChoice.main_region.State0)
	}

	@Test
	operation test24 () {
	    enter
	    raise event0
	    raise event0
	    assert is_final
	}

	@Test
	operation test25 () {
	    assert !is_active
	    assert !is_final
	}

	@Test
	operation test26 () {
	    assert !is_active
	    enter
	    assert is_active
	}

	@Test
	operation test27 () {
	    assert !is_active
	}

	@Test
	operation test28 () {
	    enter
	    raise event0
	    raise event0
	    assert is_final
	}

	@Test
	operation test29 () {
	    enter
	    raise event0
	    raise event0
	    triggerWithoutEvent
	    assert is_final
	}

	@Test
	operation test30 () {
	    enter
	    raise event0
	    raise event0
	}

	@Test
	operation test31 () {
	    enter
	    raise event0
	    raise event0
	    assert is_active
	    exit
	}

}