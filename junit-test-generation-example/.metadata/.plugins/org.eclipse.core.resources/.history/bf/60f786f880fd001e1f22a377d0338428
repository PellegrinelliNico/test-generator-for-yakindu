testclass EventsTest for statechart Events {
	@Test
	operation test00 () {
	    enter
	    raise i: 10
	    proceed 1 cycle
	    assert is_active
	}

	@Test
	operation test02 () {
	    enter
	    proceed 1 cycle
	    assert is_active
	}

	@Test
	operation test03 () {
	    raise i: 2526
	    enter
	    proceed 1 cycle
	    assert is_active
	}

	@Test
	operation test04 () {
	    exit
	    assert !is_final
	}

	@Test
	operation test05 () {
	    enter
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test06 () {
	    enter
	    assert is_active
	    assert active (Events.internal.State0)
	}

	@Test
	operation test07 () {
	    enter
	    raise i: -1337
	    proceed 1 cycle
	    assert is_active
	    assert active (Events.main_region.integer)
	}

	@Test
	operation test08 () {
	    raise b: true
	    enter
	    proceed 1 cycle
	    assert is_active
	    assert active (Events.main_region.boolean)
	}

	@Test
	operation test09 () {
	    enter
	    assert is_active
	    assert !active (Events.main_region.StateZ)
	}

	@Test
	operation test10 () {
	    enter
	    assert is_active
	    assert active (Events.main_region.StateA)
	}

	@Test
	operation test11 () {
	}

	@Test
	operation test12 () {
	    assert !active (Events.main_region.StateZ)
	}

	@Test
	operation test13 () {
	    assert !active (Events.internal.State0)
	}

	@Test
	operation test14 () {
	    assert !active (Events.main_region.StateZ.r2.StateF)
	}

	@Test
	operation test15 () {
	    assert !active (Events.main_region.StateZ.r2.StateD)
	}

	@Test
	operation test16 () {
	    assert !active (Events.main_region.integer)
	}

	@Test
	operation test17 () {
	    assert !active (Events.main_region.boolean)
	}

	@Test
	operation test18 () {
	    assert !active (Events.main_region._final_)
	}

	@Test
	operation test19 () {
	    assert !active (Events.main_region.StateZ.r1.StateE)
	}

	@Test
	operation test20 () {
	    assert !active (Events.main_region.StateB)
	}

	@Test
	operation test21 () {
	    assert !active (Events.main_region.StateZ.r1.StateC)
	}

	@Test
	operation test22 () {
	    assert !active (Events.main_region.StateA)
	}

	@Test
	operation test23 () {
	    assert !is_active
	}

	@Test
	operation test24 () {
	    assert !is_active
	    enter
	    assert is_active
	}

	@Test
	operation test25 () {
	    raise typedInEvent: 2872.74
	    assert !is_active
	}

	@Test
	operation test26 () {
	    raise str: "OK"
	    enter
	    raise b: true
	    proceed 1 cycle
	    assert is_active
	}

	@Test
	operation test27 () {
	    raise inEvent
	    assert !is_final
	}

	@Test
	operation test28 () {
	    enter
	    raise i: -1337
	    proceed 1 cycle
	    proceed 1 cycle
	    assert is_active
	}

	@Test
	operation test29 () {
	    enter
	    raise str: "OK"
	    proceed 1 cycle
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test30 () {
	    enter
	    raise i: 0
	    proceed 1 cycle
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test31 () {
	    assert !is_final
	}

	@Test
	operation test32 () {
	    enter
	    raise b: true
	    proceed 1 cycle
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test33 () {
	    raise b: false
	    enter
	    proceed 1 cycle
	    assert is_active
	}

	@Test
	operation test34 () {
	    raise str: ""
	    enter
	    proceed 1 cycle
	    assert is_active
	}

	@Test
	operation test35 () {
	    proceed 1 cycle
	    assert !is_final
	}

}