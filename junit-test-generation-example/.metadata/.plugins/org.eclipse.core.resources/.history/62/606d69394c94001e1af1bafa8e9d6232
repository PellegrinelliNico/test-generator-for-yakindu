testclass StatechartTest for statechart Statechart {

	@Test
	operation test06 () {
	    raise power_on
	    enter
	    proceed 1 cycle
	    raise analize
	    proceed 1 cycle
	    proceed 1 cycle
	    assert !is_final
	}

	@Test
	operation test07 () {
	    enter
	    raise power_on
	    proceed 1 cycle
	    proceed 1 cycle
	    assert is_active
	}

	@Test
	operation test08 () {
	    enter
	    proceed 1 cycle
	    assert is_active
	}

	@Test
	operation test10 () {
	    raise power_on
	    enter
	    proceed 1 cycle
	    raise analize
	    proceed 1 cycle
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test11 () {
	    raise power_on
	    enter
	    proceed 1 cycle
	    raise work
	    proceed 1 cycle
	    exit
	    assert !is_active
	}

	@Test
	operation test12 () {
	    enter
	    raise power_on
	    proceed 1 cycle
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test13 () {
	    enter
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test14 () {
	    enter
	    assert is_active
	    assert !active (Statechart.mainregion.on.r1.analyses)
	}

	@Test
	operation test15 () {
	    raise power_on
	    enter
	    proceed 1 cycle
	    raise work
	    proceed 1 cycle
	    assert is_active
	    assert active (Statechart.mainregion.on.r1.working)
	}

	@Test
	operation test16 () {
	    raise power_on
	    enter
	    proceed 1 cycle
	    raise analize
	    proceed 1 cycle
	    assert is_active
	    assert active (Statechart.mainregion.on)
	}

	@Test
	operation test17 () {
	    enter
	    assert is_active
	    assert !active (Statechart.mainregion.on)
	}

	@Test
	operation test18 () {
	    enter
	    raise power_on
	    proceed 1 cycle
	    assert is_active
	    assert active (Statechart.mainregion.idle)
	}

	@Test
	operation test19 () {
	    enter
	    assert is_active
	    assert active (Statechart.mainregion.off)
	}

	@Test
	operation test34 () {
	    raise power_on
	    enter
	    proceed 1 cycle
	    raise analize
	    proceed 1 cycle
	    raise end_all
	    proceed 1 cycle
	    assert is_final
	}

	@Test
	operation test38 () {
	    enter
	    assert !is_active
	}

	@Test
	operation test42 () {
	    raise power_on
	    enter
	    proceed 1 cycle
	    raise analize
	    proceed 1 cycle
	    raise end_all
	    proceed 1 cycle
	    assert is_final
	    exit
	    assert !is_active
	}

	@Test
	operation test44 () {
	    raise power_on
	    enter
	    proceed 1 cycle
	    raise work
	    proceed 1 cycle
	    raise end_all
	    proceed 1 cycle
	    assert is_final
	}

	@Test
	operation test45 () {
	    assert !is_active
	    enter
	    assert is_active
	}

}