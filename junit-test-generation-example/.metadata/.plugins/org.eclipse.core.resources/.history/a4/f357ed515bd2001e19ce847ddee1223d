testclass SimpleSeriesCycleBasedTest for statechart SimpleSeriesCycleBased {
	@Test
	operation test00 () {
	    enter
	    raise event2
	    proceed 1 cycle
	    proceed 1 cycle
	    assert is_active
	}

	@Test
	operation test01 () {
	    enter
	    raise event1
	    proceed 1 cycle
	    raise event2
	    proceed 1 cycle
	    assert is_active
	}

	@Test
	operation test02 () {
	    enter
	    proceed 1 cycle
	    assert is_active
	}

	@Test
	operation test03 () {
	    exit
	    assert !is_active
	}

	@Test
	operation test04 () {
	    raise event2
	    enter
	    proceed 1 cycle
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test05 () {
	    enter
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test06 () {
	    raise event2
	    enter
	    proceed 1 cycle
	    assert is_active
	    assert active (SimpleSeriesCycleBased.main_region.StateC)
	}

	@Test
	operation test07 () {
	    enter
	    assert is_active
	    assert active (SimpleSeriesCycleBased.main_region.StateA)
	}

	@Test
	operation test08 () {
	}

	@Test
	operation test09 () {
	    assert !active (SimpleSeriesCycleBased.main_region._final_)
	}

	@Test
	operation test10 () {
	    assert !active (SimpleSeriesCycleBased.main_region.StateB)
	}

	@Test
	operation test11 () {
	    assert !active (SimpleSeriesCycleBased.main_region.StateC)
	}

	@Test
	operation test12 () {
	    assert !active (SimpleSeriesCycleBased.main_region.StateA)
	}

	@Test
	operation test13 () {
	    proceed 1 cycle
	    assert !is_active
	}

	@Test
	operation test14 () {
	    assert !is_active
	    enter
	    raise event1
	    proceed 1 cycle
	    proceed 1 cycle
	    assert is_active
	}

	@Test
	operation test15 () {
	    enter
	    raise event2
	    proceed 1 cycle
	    raise event3
	    proceed 1 cycle
	    assert is_final
	}

	@Test
	operation test16 () {
	    assert !is_final
	    assert !is_active
	}

	@Test
	operation test17 () {
	    assert !is_active
	    enter
	    assert is_active
	}

	@Test
	operation test18 () {
	    assert !is_active
	}

	@Test
	operation test19 () {
	    enter
	    raise event1
	    proceed 1 cycle
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test20 () {
	    raise event2
	    enter
	    proceed 1 cycle
	    raise event3
	    proceed 1 cycle
	    proceed 1 cycle
	    assert is_final
	}

}