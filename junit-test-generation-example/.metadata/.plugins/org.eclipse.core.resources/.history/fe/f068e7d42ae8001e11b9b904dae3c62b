testclass OperationTestTest for statechart OperationTest {
	@Test
	operation test00 () {
	    raise a
	    raise I.b
	    proceed 1 cycle
	    enter
	}

	@Test
	operation test01 () {
	}

	@Test
	operation test02 () {
	    raise a
	    enter
	    proceed 1 cycle
	    assert is_active
	    assert active (OperationTest.main_region.StateB)
	}

	@Test
	operation test03 () {
	}

	@Test
	operation test04 () {
	    assert !active (OperationTest.main_region.StateB)
	}

	@Test
	operation test05 () {
	    assert !active (OperationTest.main_region.StateA)
	}

	@Test
	operation test06 () {
	    enter
	    assert is_active
	}

	@Test
	operation test07 () {
	    assert !is_active
	}

	@Test
	operation run () {
	    raise I.b
	    enter
	    raise a
	    proceed 1 cycle
	}

	@Test
	operation test12 () {
	    raise I.b
	    enter
	    raise a
	    proceed 1 cycle
	}

	@Test
	operation test13 () {
	    enter
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test14 () {
	    assert !is_final
	}

	@Test
	operation test15 () {
	    exit
	    assert !is_final
	}

}