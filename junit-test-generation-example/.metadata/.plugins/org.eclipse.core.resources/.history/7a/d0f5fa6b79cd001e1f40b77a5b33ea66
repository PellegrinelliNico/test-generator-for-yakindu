testclass ExamplStcTest for statechart ExamplStc {
	@Test
	operation test01 () {
	    enter
	    raise power_on
	    raise analyze
	    raise start
	    assert is_active
	    assert active (ExamplStc.main_region.On.r_0.Simplified_analyses012.r___1.an1)
	}

	@Test
	operation test03 () {
	    enter
	    raise power_on
	    raise analyze
	    raise start
	    raise Work
	    raise b12W
	    raise Work
	    assert is_active
	}

	@Test
	operation test05 () {
	    raise b12W
	    enter
	    raise power_on
	    raise Work
	    raise b12W
	    assert is_active
	}

	@Test
	operation test06 () {
	    enter
	    raise power_on
	    raise analyze
	    raise end_all
	    raise b12W
	    assert is_final
	}

	@Test
	operation test07 () {
	    enter
	    raise power_on
	    raise analyze
	    raise start
	    raise b12W
	    enter
	    triggerWithoutEvent
	    assert is_active
	}

	@Test
	operation test08 () {
	    enter
	    raise power_on
	    raise end
	    assert !is_final
	}

	@Test
	operation test09 () {
	    enter
	    raise analyze
	    assert is_active
	}

	@Test
	operation test10 () {
	    enter
	    raise power_on
	    raise Work
	    raise end_all
	    assert is_active
	}

	@Test
	operation test11 () {
	    enter
	    raise power_on
	    raise analyze
	    raise start
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test13 () {
	    enter
	    raise power_on
	    raise analyze
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test14 () {
	    enter
	    raise power_on
	    raise Work
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test15 () {
	    enter
	    raise power_on
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test16 () {
	    enter
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test18 () {
	    enter
	    raise power_on
	    raise analyze
	    raise start
	    assert is_active
	    assert active (ExamplStc.main_region.On.r_0.Simplified_analyses012)
	}

	@Test
	operation test19 () {
	    enter
	    assert is_active
	    assert !active (ExamplStc.main_region.On.r_0.Simplified_analyses012)
	}

	@Test
	operation test20 () {
	    enter
	    raise power_on
	    raise analyze
	    assert is_active
	    assert active (ExamplStc.main_region.On.r_0.Init_analyses)
	}

	@Test
	operation test21 () {
	    enter
	    raise power_on
	    raise analyze
	    assert is_active
	    assert active (ExamplStc.main_region.On)
	}

	@Test
	operation test22 () {
	    enter
	    assert is_active
	    assert !active (ExamplStc.main_region.On)
	}

	@Test
	operation test23 () {
	    enter
	    raise power_on
	    assert is_active
	    assert active (ExamplStc.main_region.idle)
	}

	@Test
	operation test24 () {
	    enter
	    assert is_active
	    assert active (ExamplStc.main_region.Off_)
	}

	@Test
	operation test28 () {
	    enter
	    raise power_on
	    raise analyze
	    raise start
	    raise a
	    assert is_active
	}

	@Test
	operation test38 () {
	    enter
	    raise power_on
	    raise analyze
	    raise start
	    raise b12W
	    assert !is_final
	}

	@Test
	operation test40 () {
	    enter
	    raise power_on
	    raise Work
	    raise end
	    assert is_active
	    assert is_final
	}

	@Test
	operation test42 () {
	    enter
	    assert is_active
	}

	@Test
	operation test43 () {
	    enter
	    raise power_on
	    raise Work
	    raise end
	    assert is_active
	    exit
	    assert !is_final
	}

	@Test
	operation test45 () {
	    enter
	    raise power_on
	    raise analyze
	    raise start
	    raise b12W
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test46 () {
	    enter
	    raise power_on
	    raise analyze
	    raise end_all
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test48 () {
	    enter
	    raise power_on
	    raise Work
	    raise end
	    raise b12W
	    assert is_active
	}

	@Test
	operation test49 () {
	    enter
	    raise power_on
	    raise analyze
	    raise start
	    raise end_all
	    assert is_final
	}

}