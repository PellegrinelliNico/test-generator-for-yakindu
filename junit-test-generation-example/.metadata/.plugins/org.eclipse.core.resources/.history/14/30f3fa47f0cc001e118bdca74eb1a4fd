testclass ExamplStcTest for statechart ExamplStc {
	@Test
	operation test00 () {
	    enter
	    raise power_on
	    raise Work
	    raise end
	    assert is_active
	    assert active (ExamplStc.main_region.On)
	}

	@Test
	operation test09 () {
	    enter
	    exit
	    assert is_active
	}

	@Test
	operation test11 () {
	    enter
	    raise power_on
	    exit
	    assert is_active
	}

	@Test
	operation test12 () {
	    enter
	    raise power_on
	    exit
	    exit
	    assert is_active
	}

	@Test
	operation test13 () {
	    enter
	    raise power_on
	    exit
	    raise start
	    exit
	    assert !is_final
	}

	@Test
	operation test14 () {
	    enter
	    raise power_on
	    exit
	    raise start
	    enter
	    raise end_all
	    assert is_final
	}

	@Test
	operation test16 () {
	    enter
	    raise power_on
	    raise Work
	    raise end
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test17 () {
	    enter
	    raise power_on
	    exit
	    raise start
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test18 () {
	    enter
	    raise power_on
	    exit
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test19 () {
	    enter
	    raise power_on
	    raise Work
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test20 () {
	    enter
	    raise power_on
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test21 () {
	    enter
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test22 () {
	    enter
	    raise power_on
	    raise Work
	    raise end_all
	    assert is_active
	    assert active (ExamplStc.main_region._final_)
	}

	@Test
	operation test23 () {
	    enter
	    raise power_on
	    raise Work
	    raise end
	    assert is_active
	    assert active (ExamplStc.main_region.On.r_0._final_)
	}

	@Test
	operation test24 () {
	    enter
	    assert is_active
	    assert !active (ExamplStc.main_region.On.r_0.Simplified_analyses012)
	}

	@Test
	operation test25 () {
	    enter
	    raise power_on
	    raise Work
	    assert is_active
	    assert active (ExamplStc.main_region.On)
	}

	@Test
	operation test26 () {
	    enter
	    assert is_active
	    assert !active (ExamplStc.main_region.On)
	}

	@Test
	operation test27 () {
	    enter
	    raise power_on
	    assert is_active
	    assert active (ExamplStc.main_region.Simplified)
	}

	@Test
	operation test28 () {
	    enter
	    assert is_active
	    assert active (ExamplStc.main_region.Off_)
	}

	@Test
	operation test32 () {
	    enter
	    raise power_on
	    exit
	    raise start
	    raise a
	    assert is_active
	}

	@Test
	operation test44 () {
	    enter
	    raise power_on
	    raise Work
	    raise end_all
	    assert is_active
	    assert is_final
	}

	@Test
	operation test46 () {
	    enter
	    raise power_on
	    raise Work
	    raise end
	    assert is_final
