	@Test
	operation test48 () {
	    enter
	    assert is_active
	}

	@Test
	operation test50 () {
	    enter
	    raise power_on
	    exit
	    raise start
	    raise end_all
	    assert is_active
	}

	@Test
	operation test52 () {
	    enter
	    raise power_on
	    raise Work
	    raise end_all
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test54 () {
	    exit
	    enter
	    raise power_on
	    raise Work
	    raise end
	    exit
	    assert is_final
	}

	@Test
	operation test55 () {
	    enter
	    raise power_on
	    raise Work
	    raise end
	    raise end_all
	    assert is_final
	}

	@Test
	operation test56 () {
	    enter
	    raise power_on
	    exit
	    raise end_all
	    exit
	    assert is_final
	}

	@Test
	operation test57 () {
	    assert !is_active
	    enter
	    raise power_on
	    exit
	    raise start
	    exit
	    assert is_active
	}

}