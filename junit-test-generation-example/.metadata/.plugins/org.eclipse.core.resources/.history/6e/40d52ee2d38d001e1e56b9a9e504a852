testclass StatechartTest for statechart Statechart {

	@Test
	operation test00 () {
	    enter
	    raise power_on
	    raise work
	    assert is_active
	    assert active (Statechart.mainregion.on)
	}

	@Test
	operation test06 () {
	    enter
	    raise power_on
	    raise analize
	    raise analize
	    assert is_active
	}

	@Test
	operation test07 () {
	    enter
	    raise power_on
	    raise analize
	    raise end
	    raise analize
	    assert is_final
	}

	@Test
	operation test08 () {
	    enter
	    raise power_on
	    raise work
	    raise analize
	    assert is_active
	}

	@Test
	operation test09 () {
	    enter
	    raise power_on
	    raise end
	    assert is_active
	}

	@Test
	operation test10 () {
	    enter
	    raise analize
	    assert is_active
	}
	
	@Test
	operation test11 () {
	    exit
	    assert !is_final
	}

	@Test
	operation test12 () {
	    enter
	    raise power_on
	    raise analize
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test13 () {
	    enter
	    raise power_on
	    raise work
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test14 () {
	    enter
	    raise power_on
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test15 () {
	    enter
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test16 () {
	    enter
	    raise power_on
	    raise analize
	    raise end
	    assert is_active
	    assert active (Statechart.mainregion._final_)
	}

	@Test
	operation test17 () {
	    enter
	    raise power_on
	    raise analize
	    assert is_active
	    assert active (Statechart.mainregion.on.r1.analyzing)
	}

	@Test
	operation test18 () {
	    enter
	    raise power_on
	    raise work
	    assert is_active
	    assert active (Statechart.mainregion.on.r1.working)
	}

	@Test
	operation test19 () {
	    enter
	    raise power_on
	    raise analize
	    assert is_active
	    assert active (Statechart.mainregion.on)
	}

	@Test
	operation test20 () {
	    enter
	    assert is_active
	    assert !active (Statechart.mainregion.on)
	}

	@Test
	operation test21 () {
	    enter
	    raise power_on
	    assert is_active
	    assert active (Statechart.mainregion.idle)
	}

	@Test
	operation test22 () {
	    enter
	    assert is_active
	    assert active (Statechart.mainregion.off)
	}

	@Test
	operation test32 () {
	    assert !is_active
	    enter
	    assert is_active
	}

	@Test
	operation test36 () {
	    enter
	    raise power_on
	    raise work
	    raise end
	    assert is_final
	}

	@Test
	operation test37 () {
	    enter
	    raise power_on
	    raise analize
	    raise end
	    assert is_final
	}

	@Test
	operation test38 () {
	    enter
	    raise power_on
	    raise work
	    raise end
	    assert is_active
	    exit
	    assert !is_active
	}

}
