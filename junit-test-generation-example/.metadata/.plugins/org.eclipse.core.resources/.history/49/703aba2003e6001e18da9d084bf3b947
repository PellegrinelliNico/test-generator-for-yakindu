testclass TemporalEvents2Test for statechart TemporalEvents2 {
	@Test
	operation test00 () {
	    assert !is_final
	    assert !is_active
	}

	@Test
	operation test01 () {
	    enter
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test02 () {
	    assert !is_active
	    enter
	    triggerWithoutEvent
	    assert is_active
	}

	@Test
	operation test05 () {
	    exit
	    triggerWithoutEvent
	    exit
	    assert !is_active
	}

	@Test
	operation test06 () {
	    assert !is_active
	}

	@Test
	operation test07 () {
	    enter
	    assert is_active
	}

	@Test
	operation test08 () {
	    triggerWithoutEvent
	    triggerWithoutEvent
	    assert !is_active
	}

	@Test
	operation test09 () {
	    proceed 500ns
	    assert !is_final
	}

	@Test
	operation test10 () {
	    assert !active (TemporalEvents2.main_region.StateA)
	}

	@Test
	operation test11 () {
	    exit
	    triggerWithoutEvent
	    proceed 500ns
	    exit
	    enter
	    assert is_active
	}

	@Test
	operation test12 () {
	    assert !active (TemporalEvents2.main_region.StateB)
	}

	@Test
	operation test13 () {
	    assert !active (TemporalEvents2.main_region.StateC)
	}

	@Test
	operation test14 () {
	    assert !active (TemporalEvents2.main_region.StateO)
	}

	@Test
	operation test15 () {
	    assert !active (TemporalEvents2.main_region.StateD)
	}

	@Test
	operation test16 () {
	    enter
	    assert is_active
	    assert active (TemporalEvents2.main_region.StateO)
	}

}