testclass ExamplStcTest for statechart ExamplStc {
	@Test
	operation test09 () {
	    enter
	    raise start
	    raise power_on
	    raise work
	    raise start
	    assert is_active
	}

	@Test
	operation test10 () {
	    enter
	    raise power_on
	    raise power_on
	    assert is_active
	}

	@Test
	operation test11 () {
	    enter
	    raise end
	    assert is_active
	}

	@Test
	operation test12 () {
	    enter
	    raise power_on
	    raise work
	    raise end_all
	    assert is_active
	}

	@Test
	operation test14 () {
	    enter
	    raise power_on
	    raise simplified
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test15 () {
	    enter
	    raise power_on
	    raise work
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test16 () {
	    enter
	    raise power_on
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test17 () {
	    enter
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test18 () {
	    enter
	    assert is_active
	    assert !active (ExamplStc.main_region.On.r_0.Simplified_analyses012)
	}

	@Test
	operation test19 () {
	    enter
	    raise power_on
	    raise work
	    assert is_active
	    assert active (ExamplStc.main_region.On.r_0.working)
	}

	@Test
	operation test20 () {
	    enter
	    raise power_on
	    raise simplified
	    assert is_active
	    assert active (ExamplStc.main_region.On)
	}

	@Test
	operation test21 () {
	    enter
	    assert is_active
	    assert !active (ExamplStc.main_region.On)
	}

	@Test
	operation test22 () {
	    enter
	    assert is_active
	    assert active (ExamplStc.main_region.Off_)
	}

	@Test
	operation test38 () {
	    enter
	    raise power_on
	    raise simplified
	    raise end_all
	    assert is_active
	    assert is_final
	}

	@Test
	operation test40 () {
	    enter
	    raise power_on
	    raise work
	    raise end
	    assert is_active
	    assert is_final
	}

	@Test
	operation test42 () {
	    enter
	    assert is_active
	}

	@Test
	operation test46 () {
	    enter
	    raise power_on
	    raise work
	    raise end
	    triggerWithoutEvent
	    assert is_active
	}

	@Test
	operation test49 () {
	    enter
	    raise power_on
	    raise work
	    raise end
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test50 () {
	    enter
	    raise power_on
	    raise simplified
	    raise end_all
	    raise start
	    assert is_final
	}

	@Test
	operation test51 () {
	    enter
	    raise power_on
	    raise simplified
	    raise start
	    raise end_all
	    assert is_final
	}

}