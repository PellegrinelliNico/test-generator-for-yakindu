testclass TemporalEventsTest for statechart TemporalEvents {
	@Test
	operation test00 () {
		proceed 1 cycle // same with 1s
		enter
		proceed 1 cycle // same with 1s
		exit
		proceed 1 cycle // same with 1s
	}
	
	
	@Test
	operation test02 () {
	    enter
	    proceed 1 cycle
	    assert is_active
	}

	@Test
	operation test03 () {
	    exit
	    assert !is_final
	}

	@Test
	operation test04 () {
	    enter
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test05 () {
	    enter
	    proceed 2s
	    proceed 1 cycle
	    assert is_active
	    assert active (TemporalEvents.main_region.StateB)
	}

	@Test
	operation test06 () {
	    enter
	    assert is_active
	    assert active (TemporalEvents.main_region.StateA)
	}

	@Test
	operation test07 () {
	}

	@Test
	operation test08 () {
	    assert !active (TemporalEvents.main_region.ERR)
	}

	@Test
	operation test09 () {
	    assert !active (TemporalEvents.main_region.StateB)
	}

	@Test
	operation test10 () {
	    assert !active (TemporalEvents.main_region._final_)
	}

	@Test
	operation test11 () {
	    assert !active (TemporalEvents.main_region.StateA)
	}

	@Test
	operation test12 () {
	    enter
	    proceed 2s
	    proceed 1 cycle
	    proceed 1 cycle
	    assert is_active
	}

	@Test
	operation test13 () {
	    proceed 1 cycle
	    assert !is_active
	}

	@Test
	operation test14 () {
	    assert !is_active
	    assert !is_final
	}

	@Test
	operation test15 () {
	    assert !is_active
	    enter
	    assert is_active
	}

	@Test
	operation test16 () {
	    assert !is_active
	}

	@Test
	operation test21 () {
	    raise stop
	    assert !is_active
	}

	@Test
	operation test22 () {
	    raise next
	    assert !is_active
	}

	@Test
	operation test23 () {
	    raise check
	    assert !is_active
	}

	@Test
	operation test24 () {
	    enter
	    proceed 2s
	    proceed 1 cycle
	    exit
	    assert !is_active
	}

}