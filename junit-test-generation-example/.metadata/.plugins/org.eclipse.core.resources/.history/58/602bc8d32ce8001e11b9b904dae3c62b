testclass TemporalEventsTest for statechart TemporalEvents {
	@Test
	operation test04 () {
	    exit
	    assert !is_final
	}

	@Test
	operation test05 () {
	    enter
	    proceed 2s
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test06 () {
	    enter
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test07 () {
	    enter
	    assert is_active
	    assert active (TemporalEvents.main_region.StateA)
	}

	@Test
	operation test08 () {
	    assert !active (TemporalEvents.main_region.ERR)
	}

	@Test
	operation test09 () {
	    assert !active (TemporalEvents.main_region._final_)
	}

	@Test
	operation test10 () {
	    assert !active (TemporalEvents.main_region.StateB)
	}

	@Test
	operation test11 () {
	}

	@Test
	operation test12 () {
	    assert !active (TemporalEvents.main_region.StateA)
	}

	@Test
	operation test14 () {
	    triggerWithoutEvent
	    assert !is_active
	}

	@Test
	operation test15 () {
	    assert !is_active
	    assert !is_final
	}

	@Test
	operation test16 () {
	    assert !is_active
	    enter
	    assert is_active
	}

	@Test
	operation test17 () {
	    assert !is_active
	}

	@Test
	operation test20 () {
	    enter
	    proceed 2s
	    raise stop
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test22 () {
	    assert !is_active
	    enter
	    proceed 2s
	    raise check
	    assert !is_final
	}

	@Test
	operation test25 () {
	    enter
	    proceed 2s
	    raise next
	    exit
	    assert !is_active
	}

}