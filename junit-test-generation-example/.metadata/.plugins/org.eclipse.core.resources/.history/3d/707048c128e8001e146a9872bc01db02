testclass OperationTestTest for statechart OperationTest {
	@Test
	operation test01 () {
	    raise I.b
	    raise a
	    proceed 1 cycle
	    enter
	}

	@Test
	operation test02 () {
	}

	@Test
	operation test03 () {
	}

	@Test
	operation test04 () {
	}

	@Test
	operation test05 () {
	}

	@Test
	operation test06 () {
	    enter
	}

	@Test
	operation test07 () {
	}

	@Test
	operation test08 () {
	    exit
	}

	@Test
	operation test09 () {
	    raise a
	    proceed 1 cycle
	    enter
	}

	@Test
	operation test14 () {
	    raise I.b
	    enter
	    raise a
	    proceed 1 cycle
	}

	@Test
	operation test15 () {
	    assert !is_final
	    assert !is_active
	}

	@Test
	operation test16 () {
	    enter
	    assert is_active
	    exit
	    assert !is_active
	}

}