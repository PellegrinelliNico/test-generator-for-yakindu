testclass StatechartTest for statechart Statechart {
	@Test
	operation test01 () {
	    enter
	    raise power_on
	    proceed 1 cycle
	    raise analize
	    proceed 1 cycle
	    raise start
	    proceed 1 cycle
	    raise b
	    proceed 1 cycle
	    enter
	    proceed 1 cycle
	    assert is_active
	}

	@Test
	operation test02 () {
	    enter
	    raise power_on
	    proceed 1 cycle
	    raise analize
	    proceed 1 cycle
	    raise start
	    proceed 1 cycle
	    raise a
	    proceed 1 cycle
	    assert is_active
	}

	@Test
	operation test03 () {
	    raise power_on
	    enter
	    proceed 1 cycle
	    raise analize
	    proceed 1 cycle
	    proceed 1 cycle
	    assert is_active
	}

	@Test
	operation test04 () {
	    raise power_on
	    enter
	    proceed 1 cycle
	    proceed 1 cycle
	    assert is_active
	}

	@Test
	operation test06 () {
	    raise power_on
	    enter
	    proceed 1 cycle
	    raise analize
	    proceed 1 cycle
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test07 () {
	    raise power_on
	    enter
	    proceed 1 cycle
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test08 () {
	    enter
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test10 () {
	    enter
	    assert is_active
	    assert !active (Statechart.mainregion.on.r1.analyses)
	}

	@Test
	operation test11 () {
	    enter
	    raise power_on
	    proceed 1 cycle
	    raise analize
	    proceed 1 cycle
	    assert is_active
	    assert active (Statechart.mainregion.on)
	}

	@Test
	operation test12 () {
	    enter
	    assert is_active
	    assert !active (Statechart.mainregion.on)
	}

	@Test
	operation test13 () {
	    raise power_on
	    enter
	    proceed 1 cycle
	    assert is_active
	    assert active (Statechart.mainregion.idle)
	}

	@Test
	operation test14 () {
	    enter
	    assert is_active
	    assert active (Statechart.mainregion.off)
	}

	@Test
	operation test27 () {
	    enter
	    raise power_on
	    proceed 1 cycle
	    raise analize
	    proceed 1 cycle
	    raise start
	    proceed 1 cycle
	    raise b
	    proceed 1 cycle
	    proceed 1 cycle
	    assert !is_final
	}

	@Test
	operation test30 () {
	    enter
	    assert is_active
	}

	@Test
	operation test31 () {
	    enter
	    raise power_on
	    proceed 1 cycle
	    raise work
	    proceed 1 cycle
	    proceed 1 cycle
	    assert is_active
	}

	@Test
	operation test32 () {
	    enter
	    raise power_on
	    proceed 1 cycle
	    raise analize
	    proceed 1 cycle
	    raise start
	    proceed 1 cycle
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test34 () {
	    enter
	    raise power_on
	    proceed 1 cycle
	    raise analize
	    proceed 1 cycle
	    raise start
	    proceed 1 cycle
	    raise b
	    proceed 1 cycle
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test36 () {
	    raise power_on
	    enter
	    proceed 1 cycle
	    raise work
	    proceed 1 cycle
	    assert is_active
	    exit
	    assert !is_active
	}

}