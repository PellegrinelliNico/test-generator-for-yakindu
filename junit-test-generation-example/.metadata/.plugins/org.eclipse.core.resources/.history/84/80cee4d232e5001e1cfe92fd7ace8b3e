testclass TemporalEvents2Test for statechart TemporalEvents2 {
	
	@Test
	operation test() {
		enter
		assert active (main_region.StateO)
		proceed 10s
		assert active (main_region.StateA)
		proceed 1s
		assert active (main_region.StateB)
		proceed 1ms
		assert active (main_region.StateC)
		proceed 1001ns
		assert active (main_region.StateD)
		proceed 1ns
		assert is_final
		assert t == 11111
		exit
	}
}
