testclass StatechartTest for statechart Statechart {

	@Test
	operation test07 () {
	    enter
	    raise power_on
	    raise analize
	    raise end_all
	    raise analize
	    assert is_active
	}

	@Test
	operation test08 () {
	    enter
	    raise power_on
	    raise end
	    assert is_active
	}

	@Test
	operation test09 () {
	    enter
	    raise start
	    assert is_active
	}

	@Test
	operation test10 () {
	    enter
	    raise power_on
	    raise analize
	    raise start
	    raise end_all
	    assert is_active
	}

	@Test
	operation test12 () {
	    enter
	    raise power_on
	    raise analize
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test13 () {
	    enter
	    raise power_on
	    raise work
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test14 () {
	    enter
	    raise power_on
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test15 () {
	    enter
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test16 () {
	    enter
	    assert is_active
	    assert !active (Statechart.mainregion.on.r1.analyses)
	}

	@Test
	operation test17 () {
	    enter
	    raise power_on
	    raise analize
	    assert is_active
	    assert active (Statechart.mainregion.on.r1.initanalyses)
	}

	@Test
	operation test19 () {
	    enter
	    raise power_on
	    raise analize
	    assert is_active
	    assert active (Statechart.mainregion.on)
	}

	@Test
	operation test20 () {
	    enter
	    assert is_active
	    assert !active (Statechart.mainregion.on)
	}

	@Test
	operation test21 () {
	    enter
	    raise power_on
	    assert is_active
	    assert active (Statechart.mainregion.idle)
	}

	@Test
	operation test22 () {
	    enter
	    assert is_active
	    assert active (Statechart.mainregion.off)
	}

	@Test
	operation test37 () {
	    enter
	    raise power_on
	    raise analize
	    raise end_all
	    assert is_active
	    assert is_final
	}

	@Test
	operation test40 () {
	    enter
	    assert is_active
	}

	@Test
	operation test44 () {
	    assert !is_active
	    enter
	    raise power_on
	    raise analize
	    raise start
	    raise analize
	    assert !is_final
	}

	@Test
	operation test45 () {
	    enter
	    raise power_on
	    raise work
	    raise end_all
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test47 () {
	    enter
	    raise power_on
	    raise analize
	    raise start
	    assert is_active
	    exit
	    assert !is_active
	}

}

