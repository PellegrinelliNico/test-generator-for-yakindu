testclass StatechartTest for statechart Statechart {

	@Test
	operation test00 () {
	    enter
	    raise power_on
	    raise analize
	    raise start
	    raise b
	    raise a
	    assert active (Statechart.mainregion.on)
	}

	@Test
	operation test11 () {
	    enter
	    raise start
	    raise power_on
	    raise analize
	    raise start
	    raise b
	    raise start
	    assert is_active
	}

	@Test
	operation test12 () {
	    enter
	    raise power_on
	    raise work
	    raise end
	    assert is_active
	}

	@Test
	operation test13 () {
	    enter
	    raise power_on
	    raise a
	    assert is_active
	}

	@Test
	operation test15 () {
	    enter
	    raise power_on
	    raise analize
	    raise start
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test16 () {
	    enter
	    raise power_on
	    raise analize
	    exit
	    assert !is_active
	}

	@Test
	operation test17 () {
	    enter
	    raise power_on
	    raise work
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test18 () {
	    enter
	    raise power_on
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test19 () {
	    enter
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test20 () {
	    enter
	    raise power_on
	    raise analize
	    raise start
	    raise a
	    assert is_active
	}

	@Test
	operation test22 () {
	    enter
	    assert is_active
	    assert !active (Statechart.mainregion.on.r1.analyses)
	}

	@Test
	operation test23 () {
	    enter
	    raise power_on
	    raise work
	    assert is_active
	    assert active (Statechart.mainregion.on.r1.working)
	}

	@Test
	operation test24 () {
	    enter
	    raise power_on
	    raise work
	    assert is_active
	    assert active (Statechart.mainregion.on)
	}

	@Test
	operation test25 () {
	    enter
	    assert is_active
	    assert !active (Statechart.mainregion.on)
	}

	@Test
	operation test26 () {
	    enter
	    raise power_on
	    assert is_active
	    assert active (Statechart.mainregion.idle)
	}

	@Test
	operation test27 () {
	    enter
	    assert is_active
	    assert active (Statechart.mainregion.off)
	}

	@Test
	operation test40 () {
	    enter
	    raise power_on
	    raise work
	    raise end_all
	    assert is_active
	    assert is_final
	}

	@Test
	operation test43 () {
	    enter
	    assert is_active
	}

	@Test
	operation test45 () {
	    enter
	    raise power_on
	    raise analize
	    raise end_all
	    raise analize
	    assert is_active
	}

	@Test
	operation test47 () {
	    enter
	    raise power_on
	    raise work
	    raise end_all
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test49 () {
	    assert !is_active
	    enter
	    raise power_on
	    raise analize
	    raise start
	    raise b
	    raise a
	    raise analize
	    assert !is_final
	}

	@Test
	operation test50 () {
	    enter
	    raise power_on
	    raise analize
	    raise start
	    raise end_all
	    assert is_final
	}

}

