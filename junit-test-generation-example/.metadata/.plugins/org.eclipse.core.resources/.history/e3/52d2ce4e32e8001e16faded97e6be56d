testclass Controller2Test for statechart Controller2 {
	@Test
	operation test01 () {
	    proceed 1 cycle
	    assert !is_final
	}

	@Test
	operation test02 () {
	    raise myEvent
	    proceed 1 cycle
	    enter
	    proceed 1 cycle
	    assert is_active
	}

	@Test
	operation test03 () {
	    exit
	    assert !is_final
	}

	@Test
	operation test04 () {
	    assert !is_active
	    enter
	    raise myEvent
	    proceed 1 cycle
	    assert active (Controller2.main_region.StateE)
	}

	@Test
	operation test05 () {
	    assert !is_active
	    enter
	    assert active (Controller2.main_region.StateA)
	}

	@Test
	operation test06 () {
	    assert !is_active
	}

	@Test
	operation test07 () {
	    assert !is_active
	    assert !active (Controller2.main_region.StateD)
	}

	@Test
	operation test08 () {
	    assert !is_active
	    assert !active (Controller2.main_region.StateC)
	}

	@Test
	operation test09 () {
	    assert !is_active
	    assert !active (Controller2.main_region.StateB)
	}

	@Test
	operation test10 () {
	    assert !is_active
	    assert !active (Controller2.main_region.StateE)
	}

	@Test
	operation test11 () {
	    assert !is_active
	    assert !active (Controller2.main_region.StateA)
	}

	@Test
	operation test12 () {
	    assert !is_active
	    enter
	    assert is_active
	}

	@Test
	operation test13 () {
	    assert !is_active
	}

	@Test
	operation test15 () {
	    enter
	    raise myEvent
	    proceed 1 cycle
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test16 () {
	    assert !is_final
	    assert !is_active
	}

	@Test
	operation test18 () {
	    enter
	    assert is_active
	    exit
	    assert !is_active
	}

}