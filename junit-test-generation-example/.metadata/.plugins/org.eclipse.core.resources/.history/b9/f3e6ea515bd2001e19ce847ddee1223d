testclass SimpleCompositeStateTest for statechart SimpleCompositeState {
	@Test
	operation test00 () {
	    enter
	    raise ev1
	    assert is_active
	    assert active (SimpleCompositeState.main_region.StateB)
	}

	@Test
	operation test01 () {
	}

	@Test
	operation test02 () {
	    enter
	    raise ev2
	    raise ev2
	    raise ev1
	    assert is_active
	}

	@Test
	operation test03 () {
	    enter
	    triggerWithoutEvent
	    assert is_active
	}

	@Test
	operation test04 () {
	    enter
	    raise ev2
	    triggerWithoutEvent
	    assert is_active
	}

	@Test
	operation test05 () {
	    exit
	    assert !is_active
	}

	@Test
	operation test06 () {
	    enter
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test07 () {
	    enter
	    raise ev2
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test08 () {
	    enter
	    raise ev1
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test09 () {
	    enter
	    raise ev1
	    raise ev1
	    assert is_active
	    assert active (SimpleCompositeState.main_region._final_)
	}

	@Test
	operation test10 () {
	    enter
	    raise ev2
	    raise ev2
	    assert is_active
	    assert active (SimpleCompositeState.main_region.StateC)
	}

	@Test
	operation test11 () {
	    enter
	    assert is_active
	    assert active (SimpleCompositeState.main_region.StateA)
	}

	@Test
	operation test12 () {
	    enter
	    raise ev2
	    assert is_active
	    assert active (SimpleCompositeState.main_region.StateB.r1.StateB2)
	}

	@Test
	operation test13 () {
	    enter
	    raise ev1
	    assert is_active
	    assert active (SimpleCompositeState.main_region.StateB.r1.StateB1)
	}

	@Test
	operation test14 () {
	    enter
	    raise ev2
	    assert is_active
	    assert active (SimpleCompositeState.main_region.StateB)
	}

	@Test
	operation test15 () {
	}

	@Test
	operation test16 () {
	    assert !active (SimpleCompositeState.main_region._final_)
	}

	@Test
	operation test17 () {
	    assert !active (SimpleCompositeState.main_region.StateC)
	}

	@Test
	operation test18 () {
	    assert !active (SimpleCompositeState.main_region.StateA)
	}

	@Test
	operation test19 () {
	    assert !active (SimpleCompositeState.main_region.StateB.r1.StateB2)
	}

	@Test
	operation test20 () {
	    assert !active (SimpleCompositeState.main_region.StateB)
	}

	@Test
	operation test21 () {
	    assert !active (SimpleCompositeState.main_region.StateB.r1.StateB1)
	}

	@Test
	operation test22 () {
	    enter
	    raise ev1
	    raise ev1
	    assert is_active
	    assert is_final
	}

	@Test
	operation test23 () {
	    assert !is_final
	}

	@Test
	operation test24 () {
	    enter
	    assert is_active
	}

	@Test
	operation test25 () {
	    assert !is_active
	}

	@Test
	operation test26 () {
	    enter
	    raise ev2
	    raise ev2
	    raise ev2
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test27 () {
	    enter
	    raise ev1
	    triggerWithoutEvent
	    assert is_active
	}

	@Test
	operation test28 () {
	    enter
	    raise ev2
	    raise ev2
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test29 () {
	    enter
	    raise ev1
	    raise ev1
	    raise ev1
	    assert is_active
	}

	@Test
	operation test30 () {
	    triggerWithoutEvent
	    assert !is_final
	}

}