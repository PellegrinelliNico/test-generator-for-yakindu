testclass TemporalEvents2Test for statechart TemporalEvents2 {
	@Test
	operation test04 () {
	    enter
	    triggerWithoutEvent
	    assert is_active
	}

	@Test
	operation test05 () {
	    enter
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test06 () {
	    exit
	    assert !is_final
	}

	@Test
	operation test07 () {
	    enter
	    assert is_active
	    assert active (TemporalEvents2.main_region.StateO)
	}

	@Test
	operation test08 () {
	    assert !active (TemporalEvents2.main_region.StateD)
	}

	@Test
	operation test09 () {
	    assert !active (TemporalEvents2.main_region.StateO)
	}

	@Test
	operation test10 () {
	    assert !active (TemporalEvents2.main_region.StateC)
	}

	@Test
	operation test11 () {
	    assert !active (TemporalEvents2.main_region.StateB)
	}

	@Test
	operation test12 () {
	}

	@Test
	operation test13 () {
	    assert !active (TemporalEvents2.main_region.StateA)
	}

	@Test
	operation test14 () {
	    proceed 1s
	    assert !is_active
	}

	@Test
	operation test15 () {
	    enter
	    proceed 10s
	    triggerWithoutEvent
	    assert is_active
	}

	@Test
	operation test16 () {
	    assert !is_active
	    enter
	    assert is_active
	}

	@Test
	operation test17 () {
	    assert !is_active
	}

	@Test
	operation test20 () {
	    assert !is_final
	    assert !is_active
	}

	@Test
	operation test21 () {
	    enter
	    proceed 10s
	    assert is_active
	    exit
	    assert !is_active
	}

}