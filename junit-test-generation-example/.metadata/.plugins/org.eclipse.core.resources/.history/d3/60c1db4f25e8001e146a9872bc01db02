testclass EasyChoiceTest for statechart EasyChoice {
	@Test
	operation test00 () {
	    raise prepare
	    raise prepare
	    proceed 1 cycle
	    exit
	    enter
	}

	@Test
	operation test01 () {
	    raise start
	    raise prepare
	    enter
	    proceed 1 cycle
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test02 () {
	    enter
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test03 () {
	    raise end
	    raise prepare
	    raise start
	    proceed 1 cycle
	    enter
	    assert is_final
	    assert active (EasyChoice.main_region._final_)
	}

	@Test
	operation test04 () {
	    raise start
	    raise prepare
	    assert !is_active
	    enter
	    proceed 1 cycle
	    assert active (EasyChoice.main_region.OneTime)
	}

	@Test
	operation test05 () {
	    assert !is_active
	    enter
	    assert active (EasyChoice.main_region.Start)
	}

	@Test
	operation test06 () {
	    assert !is_active
	    assert !active (EasyChoice.main_region._final_)
	}

	@Test
	operation test07 () {
	    assert !active (EasyChoice.main_region.A_lot_of_times)
	    assert !is_active
	}

	@Test
	operation test08 () {
	    assert !is_active
	    assert !active (EasyChoice.main_region.TwoTimes)
	}

	@Test
	operation test09 () {
	    assert !is_active
	    assert !active (EasyChoice.main_region.OneTime)
	}

	@Test
	operation test10 () {
	    assert !is_active
	}

	@Test
	operation test11 () {
	    raise prepare
	    enter
	    raise start
	    raise end
	    proceed 1 cycle
	    assert is_final
	}

	@Test
	operation test12 () {
	    assert !is_final
	    assert !is_active
	}

	@Test
	operation test13 () {
	    assert !is_active
	    enter
	    assert is_active
	}

	@Test
	operation test14 () {
	    assert !is_active
	}

	@Test
	operation test15 () {
	    enter
	    raise prepare
	    raise end
	    raise start
	    proceed 1 cycle
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test16 () {
	    enter
	    raise prepare
	    proceed 1 cycle
	}

}