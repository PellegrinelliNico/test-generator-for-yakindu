testclass ExampleStcTest for statechart ExampleStc {
	@Test
	operation test00 () {
	    enter
	    raise power_on
	    raise work
	    raise end
	    assert is_final
	    assert active (ExampleStc.main_region.On)
	}

	@Test
	operation test02 () {
	    enter
	    triggerWithoutEvent
	    assert is_active
	}

	@Test
	operation test05 () {
	    enter
	    raise power_on
	    raise analyze
	    raise start
	    raise analyze
	    assert is_active
	}

	@Test
	operation test06 () {
	    enter
	    raise power_on
	    raise analyze
	    raise analyze
	    assert is_active
	}

	@Test
	operation test07 () {
	    enter
	    raise power_on
	    raise analyze
	    raise end_all
	    raise analyze
	    assert is_active
	}

	@Test
	operation test08 () {
	    enter
	    raise power_on
	    raise end_all
	    assert is_active
	}

	@Test
	operation test09 () {
	    enter
	    raise power_on
	    raise analyze
	    raise start
	    raise b
	    raise end_all
	    assert is_active
	}

	@Test
	operation test10 () {
	    enter
	    raise power_on
	    raise analyze
	    raise start
	    enter
	    raise end_all
	    assert is_active
	}

	@Test
	operation test11 () {
	    enter
	    raise power_on
	    raise analyze
	    raise start
	    raise b
	    raise a
	    raise end_all
	    assert is_active
	}

	@Test
	operation test12 () {
	    enter
	    raise power_on
	    raise analyze
	    raise start
	    raise b
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test14 () {
	    enter
	    raise power_on
	    raise work
	    raise end
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test15 () {
	    enter
	    raise power_on
	    raise analyze
	    raise start
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test16 () {
	    enter
	    raise power_on
	    raise analyze
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test17 () {
	    enter
	    raise power_on
	    raise work
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test18 () {
	    enter
	    raise power_on
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test19 () {
	    enter
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test20 () {
	    enter
	    raise power_on
	    raise work
	    raise end_all
	    assert is_active
	    assert active (ExampleStc.main_region._final_)
	}

	@Test
	operation test21 () {
	    enter
	    raise power_on
	    raise work
	    raise end
	    assert is_active
	    assert active (ExampleStc.main_region.On.r_0._final_)
	}

	@Test
	operation test22 () {
	    enter
	    raise power_on
	    raise analyze
	    raise start
	    raise a
	    assert is_active
	}

	@Test
	operation test23 () {
	    enter
	    raise power_on
	    raise analyze
	    raise start
	    raise b
	    enter
	    raise analyze
	    assert is_active
	}

	@Test
	operation test24 () {
	    enter
	    assert is_active
	    assert !active (ExampleStc.main_region.On.r_0.analyses012)
	}

	@Test
	operation test25 () {
	    enter
	    raise power_on
	    raise analyze
	    assert is_active
	    assert active (ExampleStc.main_region.On.r_0.Init_anAlyses)
	}

	@Test
	operation test26 () {
	    enter
	    raise power_on
	    raise analyze
	    assert is_active
	    assert active (ExampleStc.main_region.On)
	}

	@Test
	operation test27 () {
	    enter
	    assert is_active
	    assert !active (ExampleStc.main_region.On)
	}

	@Test
	operation test28 () {
	    enter
	    raise power_on
	    assert is_active
	    assert active (ExampleStc.main_region.idle)
	}

	@Test
	operation test29 () {
	    enter
	    assert is_active
	    assert active (ExampleStc.main_region.Off_)
	}

	@Test
	operation test42 () {
	    enter
	    raise power_on
	    raise work
	    raise end_all
	    assert is_active
	    assert is_final
	}

	@Test
	operation test44 () {
	    enter
	    raise power_on
	    raise work
	    raise end
	    assert is_active
	    assert is_final
	}

	@Test
	operation test46 () {
	    enter
	    assert is_active
	}

	@Test
	operation test47 () {
	    enter
	    raise power_on
	    raise work
	    raise end
	    raise end_all
	    assert is_final
	}

	@Test
	operation test48 () {
	    enter
	    raise power_on
	    raise work
	    raise end_all
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test50 () {
	    enter
	    raise power_on
	    raise analyze
	    raise start
	    raise b
	    raise a
	    assert is_active
	    exit
	    assert !is_active
	}

}