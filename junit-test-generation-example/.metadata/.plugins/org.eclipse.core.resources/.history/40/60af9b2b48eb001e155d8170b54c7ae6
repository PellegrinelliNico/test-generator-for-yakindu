testclass TemporalEventsTest for statechart TemporalEvents {
	@Test
	operation test03 () {
	    proceed 1 cycle
	    proceed 2s
	    enter
	    proceed 1 cycle
	    assert is_active
	}

	@Test
	operation test04 () {
	    exit
	    assert !is_active
	}

	@Test
	operation test05 () {
	    enter
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test06 () {
	    raise stop
	    enter
	    proceed 1 cycle
	    proceed 2s
	    assert is_active
	    assert active (TemporalEvents.main_region._final_)
	}

	@Test
	operation test07 () {
	    assert !is_active
	}

	@Test
	operation test08 () {
	    assert !active (TemporalEvents.main_region.ERR)
	    assert !is_active
	}

	@Test
	operation test09 () {
	    assert !active (TemporalEvents.main_region._final_)
	    assert !is_active
	}

	@Test
	operation test10 () {
	    assert !active (TemporalEvents.main_region.StateB)
	    assert !is_active
	}

	@Test
	operation test11 () {
	    assert !active (TemporalEvents.main_region.StateA)
	    assert !is_active
	}

	@Test
	operation test12 () {
	    raise stop
	    proceed 2s
	    proceed 1 cycle
	    enter
	    assert is_final
	}

	@Test
	operation test13 () {
	    assert !is_final
	    assert !is_active
	}

	@Test
	operation test14 () {
	    assert !is_active
	    enter
	    assert is_active
	}

	@Test
	operation test15 () {
	    assert !is_active
	}

	@Test
	operation test20 () {
	    raise stop
	    proceed 2s
	    enter
	    proceed 1 cycle
	    assert is_active
	    exit
	    assert !is_final
	}

	@Test
	operation test21 () {
	    raise check
	    proceed 2s
	    assert !is_active
	    enter
	    proceed 1 cycle
	    assert !is_final
	}

	@Test
	operation test22 () {
	    raise next
	    proceed 2s
	    assert !is_active
	    enter
	    proceed 1 cycle
	    assert is_active
	}

}